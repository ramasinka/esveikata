<?xml version="1.0" encoding="UTF-8"?>
<List xmlns="http://hl7.org/fhir" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.w3.org/2005/Atom http://www.hl7.org/implement/standards/fhir/immunization.xsd">
    <code>
        <!-- 1..1 -->
        <!-- Sąrašo tipas / List type -->
        <!-- Classificator: -->
        <coding>
            <system value="http://loinc.org"/>
            <code value="11535-2"/>
        </coding>
    </code>
    
    <!-- 1..1 -->
    <!-- Pacientas, su kuriuo yra susiję sąraše esantys resursai / Patient who is a subject of listed resources -->
    <!-- Data validation DTT.6. -->
    <subject>
        <reference value="Patient/123456789/_history/1"/>
    </subject>
    
    <!-- 1..1 --> <!-- Nenaudojamas laukas, tačiau privalomas / Not used, but mandatory --> 
    <!-- The processing mode that applies to this list  -->
    <!-- Values: working (by default) -->
    <mode value="working"/>

    <!-- 0..* -->
    <entry>
        <item>
            <!-- Nuoroda į sąraše esantį resursą / Reference to listed resource -->
            <reference value="Condition/123456/_history/1"/>
        </item>
    </entry>
</List>
