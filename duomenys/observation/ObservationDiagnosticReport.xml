<?xml version="1.0" encoding="UTF-8"?>
<Observation xmlns="http://hl7.org/fhir" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.w3.org/2005/Atom http://www.hl7.org/implement/standards/fhir/observation.xsd">
    
    <!-- 0..1 -->
    <!-- Naudotas analizatorius (aparatūra) / Used analyzer -->
    <!-- data-element-1411 -->
    <extension url="http://esveikata.lt/Profile/ltnhr-observation#analyserEquipmentUsed">
        <valueString value="Some analyzer" />
    </extension>
    
    <name>
        <!-- 1..1 -->
        <!-- Classificator: -->
        <coding>
            <system value="http://loinc.org"/>
            <code value="52482-7"/>
        </coding>
    </name>
    
    <!-- 1..1 -->
    <!-- Tyrimo atlikimo data ir laikas / Date and time when laboratory test performed -->
    <!-- data-element-1430 -->
    <appliesDateTime value="2012-09-17T16:13:54.031+02:00"/>
    
    <!-- 1..1 -->
    <!-- Apžiūros duomenų būsena "final" arba "entered in error" atšauktiems duomenims / "final" or "entered in error" for canceled -->
    <!-- Classificator: observation-status -->
    <status value="final"/>
    
    <!-- 1..1 -->
    <!-- Logikoje nenaudojamas, nurodoma pagal nutylėjimą "ok" / Not used field, so "ok" by default -->
    <reliability value="ok"/>
    
    <!-- 1..1 -->
    <!-- Taikytas analizės metodas / Used analyze method -->
    <!-- data-element-1420 -->
    <method>
        <text value="Taikytas analizės metodas"/>
    </method>
    
    <!-- 0..* -->
    <!-- Tirtos analitės / Analytes -->
    <related>
        <!-- 1..1 -->
        <!-- Nuoroda į tirtos analitės duomenų resursą / Reference to the analyte resource -->
        <target>
            <reference value="Observation/3320321/_history/1"/>
        </target>
    </related>
</Observation>