<?xml version="1.0" encoding="UTF-8"?>
<!-- Anamnezės pavyzdys / Anamnesis sample -->
<Observation xmlns="http://hl7.org/fhir" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.w3.org/2005/Atom http://www.hl7.org/implement/standards/fhir/observation.xsd">
    <name>
        <coding>
            <!-- 1..1 -->
            <!-- Classificator: observation-code -->
            <system value="http://loinc.org"/>
            <code value="11348-0"/>
        </coding>
    </name>
    
    <!-- 1..1 -->
    <!-- Anamnezės aprašymas / Anamnesis description -->
    <valueString value="Anamnezės aprašymas as text."/>
    
    <!-- 1..1 -->
    <!-- Apžiūros duomenų būsena "final" arba "entered in error" atšauktiems duomenims / "final" or "entered in error" for canceled -->
    <!-- Classificator: observation-status -->
    <status value="final"/>
    
    <!-- 1..1 -->
    <!-- Logikoje nenaudojamas, nurodoma pagal nutylėjimą "ok" / Not used field, so "ok" by default -->
    <!-- Classificator: observation-reliability -->
    <reliability value="ok"/>
    
    <!-- 1..1 -->
    <!-- Pacientas, kurio duomenys yra aprašomi / Patient who observation is being made -->
    <subject>
        <reference value="Patient/32856257683256/_history/2"/>
    </subject>
</Observation>