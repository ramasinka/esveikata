<Organization xmlns="http://hl7.org/fhir" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.w3.org/2005/Atom http://www.hl7.org/implement/standards/fhir/organization.xsd">

	<!-- 1..* -->
	<!-- Įstaigos licencijų duomenys / Organization licenses -->
	<extension url="http://esveikata.lt/Profile/ltnhr-organization#licenses">

		<!-- 1..1 -->
		<!-- Licencijos numeris / License number -->
		<extension url="http://esveikata.lt/Profile/ltnhr-organization#licenses.number">
			<valueIdentifier>
				<system value="http://esveikata.lt/Identifier/OrganizationLicenseNumber"/>
				<value value="862"/>
			</valueIdentifier>
		</extension>

		<!-- 0..1 -->
		<!-- Classificator: organization-license-type -->
		<!-- Licencijos rūšis / License type -->
		<extension url="http://esveikata.lt/Profile/ltnhr-organization#licenses.type">
			<valueCodeableConcept>
				<coding>
					<system value="http://esveikata.lt/classifiers/OrganizationLicenseType"/>
					<code value="PHARMACY"/>
					<display value="Vaistinės veiklos licencija"/>
				</coding>
			</valueCodeableConcept>
		</extension>

		<!-- 0..1 -->
		<!-- Classificator: controlled-substances-company-license-type -->
		<!-- Papildomos licencijos rūšis / Additional license type -->
		<extension url="http://esveikata.lt/Profile/ltnhr-organization#licenses.additionalLicenseType">
			<valueCodeableConcept>
				<coding>
					<system value="http://esveikata.lt/classifiers/OrganizationAdditionalLicenseType"/>
					<code value="MANUFACTURE_SCHEDULE_II_III"/>
					<display value="Licencija gaminti II ir III sąrašų narkotines ir psichotropines medžiagas"/>
				</coding>
			</valueCodeableConcept>
		</extension>

		<!-- 1..1 -->
		<!-- Licencijos išdavimo data / License issue date -->
		<extension url="http://esveikata.lt/Profile/ltnhr-organization#licenses.grantingDate">
			<valueDate value="2010-01-20"/>
		</extension>

		<!-- 1..1 -->
		<!-- Classificator: license-status -->
		<!-- Licencijos būsena / License status -->
		<extension url="http://esveikata.lt/Profile/ltnhr-organization#licenses.status">
			<valueCodeableConcept>
				<coding>
					<system value="http://esveikata.lt/classifiers/OrganizationLicenseStatus"/>
					<code value="VALID"/>
					<display value="Galiojanti"/>
				</coding>
			</valueCodeableConcept>
		</extension>

		<!-- 0..1 -->
		<!-- Licencijos būsenos pasikeitimo data / Date when license status was changed -->
		<extension url="http://esveikata.lt/Profile/ltnhr-organization#licenses.statusChangedDate">
			<valueDate value="2010-01-20"/>
		</extension>
	</extension>

	<!-- 0..1 -->
	<!-- Sveikatinimo įstaigos kodas (JAR) / Registration Code -->
	<!-- data-element-280  -->
	<identifier>
		<system value="http://esveikata.lt/Identifier/JAR"/>
		<value value="135163499"/>
	</identifier>

	<!-- 1..1 -->
	<!-- Sveikatos priežiūros organizacinio vieneto (įstaigos, jos filialo, skyriaus, filialo skyriaus ir pan.) kodas SVEIDRA IS sistemoje / SVEIDRA ID issued by National health insurance fund -->
	<!-- data-element-297 -->
	<identifier>
		<system value="http://esveikata.lt/Identifier/SVEIDRAID"/>
		<value value="4699"/>
	</identifier>

	<!-- 0..1 -->
	<!-- Licencijos veiklos vietos numeris / Number of license activity place -->
	<identifier>
		<system value="http://esveikata.lt/Identifier/ActivityPlaceNumber"/>
		<value value="862/01"/>
	</identifier>

	<!-- 0..1 -->
	<!-- Sveikatos priežiūros įstaigos pavadinimas / HCP name -->
	<!-- data-element-270 -->
	<name value="Hospital of Lithuanian University of Health Sciences Kaunas Clinics"/>

	<!-- 1..1 -->
	<!-- Organizacijos tipas: pagrindinis sveikatos priežiūros paslaugų teikėjas (visa ligoninė) arba jos skyrius / Type of organization: Main Healthcare Provider (whole Hospital) or its department -->
	<type>
		<coding>
			<system value="http://hl7.org/fhir/organization-type"/>
			<!-- Values: "prov" : Sveikatos priežiūros paslaugų teikėjas / Healthcare Provider | "dept" - Ligoninės skyrius / Hospital Department -->
			<code value="dept"/>
		</coding>
	</type>

	<!-- 0..1 -->
	<!-- Sveikatinimo įstaigos telefono numeris / HCP telephone number -->
	<telecom>
		<system value="phone"/>
		<value value="+37067800000"/>
	</telecom>

	<!-- 0..1 -->
	<!-- Sveikatinimo įstaigos telefono numeris / HCP telephone number -->
	<telecom>
		<system value="phone"/>
		<value value="+37067811111"/>
		<use value="mobile"/>
	</telecom>

	<!-- 0..1 -->
	<!-- Sveikatinimo įstaigos fakso numeris / HCP Fax number -->
	<telecom>
		<system value="fax"/>
		<value value="+37067822222"/>
	</telecom>

	<!-- 0..1 -->
	<!-- Sveikatinimo įstaigos el. pašto adresas / HCP email -->
	<telecom>
		<system value="email"/>
		<value value="organization@domain.lt"/>
	</telecom>

	<!-- 0..1 -->
	<!-- Licencijos veiklos vietos adresas / Address of license activity place -->
	<address>
		<text value="Alšėnų g.2A, Kaunas"/>
	</address>

	<!-- 0..1 -->
	<!-- Tuo atveju jei ligoninės skyrius, nuoroda į tėvinį skyrių ar sveikatinimo organizaciją / In case of department, reference to parent department or health organization -->
	<partOf>
		<reference value="Organization/123/_history/1"/>
	</partOf>
</Organization>
