<?xml version="1.0" encoding="UTF-8"?>
<Organization xmlns="http://hl7.org/fhir"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.w3.org/2005/Atom http://www.hl7.org/implement/standards/fhir/organization.xsd">
    <!-- 1..1 -->
    <!-- Įstaigos (padalinio) SVEIDRA ID / Department ID issued by National health insurance fund -->
    <!-- data-element-297 -->
    <identifier>
        <system value="http://esveikata.lt/Identifier/SVEIDRAID"/>
        <value value="123456789"/>
    </identifier>
    
    <!-- 0..1 -->
    <!-- Skyriaus pavadinimas / Department name -->
    <!-- data-element-270 -->
    <name value="Department of Neurology"/>
    
    <!-- 1..1 -->
    <!-- Organizacijos tipas: pagrindinis sveikatos priežiūros paslaugų teikėjas (visa ligoninė) arba jos skyrius / Type of organization: Main Healthcare Provider (whole Hospital) or its department -->
    <type>
        <coding>
            <system value="http://hl7.org/fhir/organization-type"/>
            <!-- Values: "prov" : Sveikatos priežiūros paslaugų teikėjas / Healthcare Provider | "dept" - Ligoninės skyrius / Hospital Department -->
            <code value="dept"/>
        </coding>
    </type>
    
    <!-- 0..1 -->
    <!-- Tuo atveju, jei ligoninės skyrius, nuoroda į tėvinį skyrių ar sveikatinimo organizaciją / In case of department, reference to parent department or health organization -->
    <partOf>
        <reference value="Organization/123/_history/1"/>
    </partOf>
</Organization>