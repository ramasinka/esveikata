                    <!-- 0..1 -->
                    <!-- NPAKID-7  / NPAKID  -->
                    <!-- 0..* -->
                    <coding>
                        <system value="http://esveikata.lt/Identifier/Medication/NPAKID-7"/>
                        <code value="1020497"/>
                    </coding>
                    <coding>
                        <system value="http://esveikata.lt/Identifier/Medication/NPAKID"/>
                        <code value="20497"/>
                    </coding>
