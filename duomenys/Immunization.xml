<?xml version="1.0" encoding="UTF-8"?>
<!-- Skiepo aprašymas / Immunization description -->
<Immunization xmlns="http://hl7.org/fhir" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.w3.org/2005/Atom http://www.hl7.org/implement/standards/fhir/immunization.xsd">

    <!-- 1..1 -->
    <!-- Vakcinos pakuotės duomenys / Vaccine package info -->
    <!-- data-element-1030 -->
    <extension url="http://esveikata.lt/Profile/ltnhr-immunization#vaccineMedicationPackage">
        <valueResource>
            <reference value="Medication/1/_history/1"/>
        </valueResource>
    </extension>

    <!-- 1..1 -->
    <!-- 6.3.12 Skiepijimo data / Date of vaccination -->
    <!-- data-element-1060 -->
    
    <date value="2013-01-10"/>

    <!-- 1..1 -->
    <!-- Nurodomas NPAKID-7 arba NPAKID identifikatorius (-iai) iš vakcinos pakuotes vaccineMedicationPackage.Medication.code lauko / code value from vaccineMedicationPackage.Medication.code field -->
    <!-- data-element-1020 -->
    <vaccineType>
        <!-- 0..1 -->
        <!-- NPAKID-7  / NPAKID  -->
        <!-- 0..* -->
        <coding>
            <system value="http://esveikata.lt/classifiers/Identifier/NPAKID-7"/>
            <code value="1020497"/>
        </coding>
        <coding>
            <system value="http://esveikata.lt/classifiers/Identifier/NPAKID"/>
            <code value="20497"/>
        </coding>
    </vaccineType>

    <!-- 1..1 -->
    <!-- Pacientas, kuris skiepijamas / Patient whom vaccination -->
    
    <subject>
        <reference value="Patient/32856257683256/_history/2"/>
    </subject>

    <!-- 1..1 -->
    <!-- Požymis ar vakcinacija buvo atmesta. Laukas nenaudojamas, bet yra privalomas pagal FHIR specifikaciją./ Indicates if the vaccination was refused (not used but mandatory by FHIR specification) -->
    <!-- Values: "false" (default) : neatmesta, "true" : atmesta  -->
    <refusedIndicator value="false"/>

    <!-- 1..1 -->
    <!-- Nurodo ar tai tik informacija apie vakcinavimo duomenis, ar vakcinavimas buvo iš tiesų atliktas pacientui. Laukas nenaudojamas, bet privalomas pagal FHIR specifikaciją. /True if this administration was reported rather than directly performed vaccination -->
    <!-- Pagal nutylėjimą visada "false" /  false value by default -->
    <reported value="false"/>

    <!-- 1..1 -->
    <!-- Specialistas, kuris atliko skiepą / Specialist who performed vaccination -->
    
    <performer>
        <reference value="Practitioner/321654/_history/1"/>
    </performer>

    <!-- 1..1 -->
    <!-- 6.2.3 Nuoroda į specialisto, kuris siuntė, resursą / Reffereal practitioner -->
    <requester>
        <reference value="Practitioner/123456/_history/1"/>
    </requester>

    <!-- 1..1 -->
    <!-- 6.3.8	Vaisto tinkamumo vartoti laikas / Vaccine expiration date -->
    <expirationDate value="2015-02-15"/>

    <!-- 1..1 -->
    <!-- 6.3.11 Įskiepijimo būdas / Vaccination method -->
    <!-- data-element-1050 -->
    <!-- Classificator: immunization-route -->
    <route>
        <coding>
            <system value="http://esveikata.lt/classifiers/Immunization/Route"/>
            <!-- Values: "1" : į burną | "2" : į nosį | "3" : į odą | "4" : į paodį | "5" : į raumenis -->
            <code value="3"/>
            <display value="Į odą"/>
        </coding>
    </route>

    <!-- 0..1 -->
    <!-- 6.3.15 Reakcija į skiepą / Reaction to vaccination -->
    <!-- data-element-1090 -->
    <reaction>
        <detail>
            <reference value="cid:4"/>
        </detail>
    </reaction>

    <vaccinationProtocol>
        <!-- 1..1 -->
        <!-- 6.3.10 Dozės eilės numeris / Dose serial number -->
        
        <doseSequence value="1"/>

        <!-- 1..1 -->
        <!-- 6.3.13 Įskiepyta vakcinos dozė / Dose of vaccine -->
        <!-- data-element-1070 -->
        <description value="[string]"/>

        <!-- 1..1 -->
        <!-- 6.3.14 Serija / Vaccine serie -->
        <!-- data-element-1080 -->
        <series value="Serie description as text."/>

        <doseTarget>
            <!-- 1..* -->
            <!-- 6.3.1 Užkrečiamųjų ligų, nuo kurių skiepijama, kodai ir pavadinimai / Contagious disease code and description -->
            <!-- Classificator: infectious-disease -->
            
            <coding>
                <system value="http://esveikata.lt/classifiers/infectious-disease"/>
                <code value="A36"/>
                <display value="Difterija"/>
            </coding>

            <!-- 6.3.1.	Užkrečiamųjų ligų, nuo kurių skiepijama, pavadinimai laisvu tekstu -->
            <text value="Difterija"/>
        </doseTarget>

        <!-- 1..1 -->
        <!-- Požymis ar skiepijimo faktas aktualus / Is vaccination fact actual -->
        <doseStatus>
            <coding>
                <system value="http://hl7.org/fhir/vaccination-protocol-dose-status"/>
                <!-- Values: "count" : taip | "nocount" : ne -->
                <code value="count"/>
            </coding>
        </doseStatus>
    </vaccinationProtocol>
</Immunization>
