<?xml version="1.0" encoding="UTF-8"?>
<ImagingStudy xmlns="http://hl7.org/fhir" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.w3.org/2005/Atom http://www.hl7.org/implement/standards/fhir/imagingstudy.xsd">

	<!-- 1..1 -->
	<!-- Požymis, ar tyrimas yra aprašytas / Study reported flag -->
	<extension url="http://esveikata.lt/Profile/ltnhr-imagingstudy#reported">
		<valueBoolean value="false" />
	</extension>

	<!-- 1..1 -->
	<!-- Tyrimo atlikimo laikas / study date -->
	<dateTime value="2014-11-04T11:01:20+11:00"/>
	
	<!-- 1..1 -->
	<!-- Nuoroda į Paciento resursą / Reference to Patient resource -->
	<subject>
		<reference value="Patient/123456789/_history/1"/>
	</subject>
	
	<!-- 1..1 -->
	<!-- Unikalus tyrimo identifikatorius / Unique study id -->
	<uid value="urn:oid:1.2.840.113619.2.327.3.4271022949.94.1410959782.326"/>
	
	<!-- 1..1 -->
	<!-- Serijų skaičius tyrime / Number of series in study -->
	<numberOfSeries value="2"/>
	
	<!-- 1..1 -->
	<!-- Vaizdų skaičius tyrime / Number of images in study -->
	<numberOfInstances value="3"/>
	
	<!-- 0..1 -->
	<!-- Tyrimo apibūdinimas, pateikiamas gydytojo / Description of study made by practitioner -->
	<description value="Galva su kaklu."/>
	
	<!-- 0..* -->
	<!-- Tyrimo serija / Series of study -->
	<series>
		
		
		
		<!-- 0..1 -->
		<!-- Serijos eilės numeris tyrime / Sequence number of series in study -->
		<number value="1"/>
		
		<!-- 1..1 -->
		<!-- Tyrimo serijos tipas / Modality of series -->
		<modality value="CT"/>
		
		<!-- 1..1 -->
		<!-- Tyrimo serijos identifikatorius / Series of study unique id -->
		<uid value="urn:oid:1.2.840.113619.2.327.3.4271022949.94.1410959782.332"/>
		
		<!-- 0..1 -->
		<!-- Tyrimo serijos apibūdinimas / Series of study description -->
		<description value="Vaizdas iš galo"/>
		
		<!-- 1..1 -->
		<!-- Vaizdų skaičius serijoje / Number of images in series -->
		<numberOfInstances value="2"/>
		
		<!-- DICOM mapping (0018,0015) -->
		<bodySite>
			<code value="T-D1000"/>
		</bodySite>
		
		<!-- 1..* -->
		<!-- Serijos vaizdas / Image of series -->
		<instance>
			
			<!-- 0..1 -->
			<!-- Vaizdo eilės numeris / Sequence number of image in series -->
			<number value="1"/>
			
			<!-- 1..1 -->
			<!-- Tyrimo vaizdo identifikatorius / Unique id of image-->
			<uid value="urn:oid:1.2.840.113619.2.327.3.4271022949.94.1410959782.334.1"/>
			
			<!-- 1..1 -->
			<!-- Tyrimo SOP klasės UID / Study SOP class unique id-->
			<sopclass value="urn:oid:1.2.840.10008.5.1.4.1.1.2"/>
		
		</instance>
		
		<instance>
			<number value="2"/>
			
			<uid value="urn:oid:1.2.840.113619.2.327.3.4271022949.94.1410959782.334.2"/>
			
			<sopclass value="urn:oid:1.2.840.10008.5.1.4.1.1.2"/>
		</instance>
		
		<!-- 0..1 -->
		<!-- Tirta kūno dalis, pagal DICOM protokolą / Body part examined according to DICOM -->
		
		
	</series>
	
	<series>
		<number value="2"/>
		
		<modality value="CT"/>
		
		<uid value="urn:oid:1.2.840.113619.2.327.3.4271022949.94.1410959782.329"/>
		
		<description value="Vaizdas iš priekio"/>
		
		<numberOfInstances value="1"/>
		
		<!-- DICOM mapping (0018,0015) -->
		<bodySite>
			<code value="T-D1000"/>
		</bodySite>
		
		<instance>
			<number value="1"/>
			
			<uid value="urn:oid:1.2.840.113619.2.327.3.4271022949.94.1410959782.331.1"/>
			
			<sopclass value="urn:oid:1.2.840.10008.5.1.4.1.1.2"/>
		</instance>
	</series>

</ImagingStudy>