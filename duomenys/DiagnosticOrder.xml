<?xml version="1.0" encoding="UTF-8"?>
<DiagnosticOrder xmlns="http://hl7.org/fhir" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.w3.org/2005/Atom http://www.hl7.org/implement/standards/fhir/diagnosticorder.xsd">

    <!-- 1..1 -->
    <!-- Nuoroda į pacientą, kuriam yra skyrimas / Reference to the patient -->
    <subject>
        <reference value="Patient/123456789"/>
    </subject>

    <!-- 0..1 -->
    <!-- Nuoroda į specialistą, kuris užsakė laboratorinį tyrimą / Reference to the specialist who ordered the laboratory test -->
    <orderer>
        <reference value="Practitioner/987654321/_history/1"/>
    </orderer>

    <!-- 1..1 -->
    <!-- Papildomas aprašymas laboratorijai / Additional details for the laboratory -->
    <!-- data-element-1250 -->
    <clinicalNotes value="Additional Details for the Laboratory"/>
    
    <!-- 1..1 -->
    <!-- Tyrimo užsakymo būsena -->
    <status value="requested"/>

    <item>
        <code>
            <!-- 1..1 -->
            <!-- Tyrimų pavadinimų klasifikatorius / Diagnostic names clasificator -->
            <!-- Classificator: diagnostic-type -->
            <coding>
                <system value="http://esveikata.lt/classifiers/DiagnosticOrder/ItemCode"/>
                <code value="123546789"/>
            </coding>

            <!-- Tyrimo pavadinimas / Diagnostic name -->
            <text value="Lipid Panel"/>
        </code>

        <!-- 1..1 -->
        <!-- 7.2.2.	Tiriamasis ėminys arba tiriamoji medžiaga / Specimen data -->
        <!-- data-element-1220 -->
        <specimen>
            <reference value="Specimen/123456789/_history/1"/>
        </specimen>
    </item>
</DiagnosticOrder>
