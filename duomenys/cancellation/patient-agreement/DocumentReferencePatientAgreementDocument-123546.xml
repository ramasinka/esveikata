<?xml version="1.0" encoding="UTF-8"?>
<!-- Sutikimo dokumentas / -->
<DocumentReference xmlns="http://hl7.org/fhir" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.w3.org/2005/Atom http://www.hl7.org/implement/standards/fhir/documentreference.xsd">

    <!-- Sutikimo numeris, nenaudojamas, bet privalomas, todėl nurodom tik privalomus duomenis, kaip nurodyta žemiau / Document number, not used but mandatory, so submiting default values as shown bellow -->
    <masterIdentifier>
        <use value="temp"/>
        <system value="http://esveikata.lt/Identifier/PatientAgreement"/>
        <value value="12345689"/>
    </masterIdentifier>

    <!-- 1..1 -->
    <!-- Nuoroda į paciento, kuriam galioja sutikimas  -->
    <subject>
        <reference value="Patient/123456/_history/1"/>
    </subject>

    <!-- 1..1 -->
    <!-- Sutikimo tipas -->
    <!-- Classificator: patient-agreement-type -->
    <type>
        <coding>
            <system value="http://esveikata.lt/classifiers/PatientAgreement/Type"/>
            <code value="4"/>
            <display value="Sutikimas dėl sveikatos paslaugų teikimo"/>
        </coding>
    </type>

    <!-- 1..1 -->
    <!-- Sutikimą užregistravęs specialistas -->
    <author>
        <reference value="Practitioner/12346/_history/1"/>
    </author>
    
    <!-- 0..1 -->
    <!-- Asmuo, kuris pateikė sutikimą už pacientą, jeigu pateikė ne pats pacientas. -->
    <author>
        <reference value="RelatedPerson/12346/_history/1"/>
    </author>

    <!-- 1..1 -->
    <!-- Data, kada buvo sukurtas šis sutikims / When this reference was created -->
    <indexed value="2005-12-24T09:43:41+11:00"/>

    <!-- 1..1 --> <!-- Nenaudojamas laukas, tačiau privalomas / Not used, but mandatory --> 
    <!-- Nuorodos būsena / The status of this document reference -->
    <!-- Values: "current" (by default) -->
    <status value="current"/>
    
    <!-- 1..1 -->
    <!-- Dokumento būsena, galimos reikšmės: final-pateiktam sutikimui, entered in error - atšauktam sutikimui / state of the agreement: final-submitted agreement, entered in error for cancelled. -->
    <docStatus>
        <coding>
            <system value="http://esveikata.lt/classifiers/Document/Status"/>
            <code value="entered in error"/>
        </coding>
    </docStatus>

    <!-- 0..1 -->
    <!-- Aprašymas -->
    <description value="Some description."/>

    <!-- 1..1 -->
    <!-- The mime type of the source document -->
    <!-- Values: "text/plain" (by default) -->
    <mimeType value="text/plain"/>
    
    <!-- 0..1 -->
    <!-- Nuoroda į binarinį sutikimo dokumentą -->
    <location value="Binary/123456879"/>

    <!-- 0..1 -->
    <!-- Sutikimo galiojimo intervalas -->
    <context>
        <period>
            <start value="2004-12-23T08:01:00"/>
            <end value="2004-12-23T08:01:00"/>
        </period>
    </context>
</DocumentReference>