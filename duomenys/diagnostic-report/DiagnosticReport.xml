<?xml version="1.0" encoding="UTF-8"?>
<DiagnosticReport xmlns="http://hl7.org/fhir" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.w3.org/2005/Atom http://www.hl7.org/implement/standards/fhir/diagnosticreport.xsd">

    <!-- 0..1 -->
    <!-- Laboratorijos pavadinimas / Laboratory name -->
    <extension url="http://esveikata.lt/Profile/ltnhr-diagnosticreport#laboratoryName">
        <valueString value="Laboratory name" />
    </extension>

    <name>
        <!-- 1..1 -->
        <!-- Tyrimų pavadinimų klasifikatorius / Tests descriptions classificator -->
        <coding>
            <system value="http://esveikata.lt/classifiers/DiagnosticOrder/ItemCode"/>
            <code value="123546789"/>
        </coding>
        
        <!-- Tyrimo pavadinimas / Test description -->
        <text value="Lipid Panel"/>
    </name>

    <!-- 1..1 -->
    <!-- Apžiūros duomenų būsena "final" arba "entered in error" atšauktiems duomenims / "final" or "entered in error" for canceled -->
    <status value="final"/>
    
    <!-- 1..1 -->
    <!-- Tyrimo atsakymo data / Test result date -->
    <!-- data-element-1611 -->
    <issued value="2013-03-11T10:28:00+01:00"/>

    <!-- 1..1 -->
    <!-- Informacija apie pacientą, kuriam atliktas tyrimas / Patient details -->
    
    <subject>
        <reference value="Patient/123456798"/>
    </subject>

    <!-- 1..1 -->
    <!-- Informacija apie sveikatos priežiūros specialistą, kuris atsakingas už tyrimą / Information about Health Care Specialist who is responsible for issuing the report. Need to know whom to contact if there are queries about the results -->
    
    <performer>
        <reference value="Practitioner/12356789"/>
    </performer>
    
    <!-- 0..1 -->
    <!-- Privalomas E200-ats medicininiame dokumente / Mandatory in E200-ats doc -->
    <!-- Patologijos tyrimo Nr. / Pathology test number -->
    <!-- data-element-1620 -->
    <identifier>
        <system value="http://esveikata.lt/Identifier/PathologyTestNumber"/>
        <value value="1234567978"/>
    </identifier>

    <!-- 0..1 -->
    <!-- Paskyrimo duomenys / Diagnostic Order data -->
    <requestDetail>
        <reference value="DiagnosticOrder/123456789/_history/1"/>
    </requestDetail>

    <!-- 1..1 -->
    <!-- Tyrimo atlikimo data ir laikas. / The date and time when the test was performed -->
    <diagnosticDateTime value="2013-03-11T03:45:00+01:00"/>

    <!-- 0..1 -->
    <!-- Tiriamasis ėminys arba tiriamoji medžiaga / Specimen data -->
    <specimen>
        <reference value="Specimen/123456789/_history/1"/>
    </specimen>

    <!-- 0..1 -->
    <!-- Nuoroda į tyrimo resursą/ Reference to the test resource -->
    <result>
        <reference value="Observation/123456789/_history/1"/>
    </result>
</DiagnosticReport>
