<?xml version="1.0" encoding="UTF-8"?>
<Patient xmlns="http://hl7.org/fhir" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.w3.org/2005/Atom http://www.hl7.org/implement/standards/fhir/patient.xsd">

    <!-- 0..1 -->
    <!-- Piliečio veido atvaizdas / Patient photo -  -->
    <extension url="http://esveikata.lt/Profile/ltnhr-patient#binaryPhoto">
        <valueResource>
            <reference value="Media/123546789"/>
        </valueResource>
    </extension>

    <!-- 0..* -->
    <!-- Asmens tapatybę patvirtinančio dokumento informacija / The identity document information -->
    <extension url="http://esveikata.lt/Profile/ltnhr-patient#identityDocument">
        <valueResource>
            <reference value="DocumentReference/123456789"/>
        </valueResource>
    </extension>

    <!-- 0..* -->
    <!-- Europos sveikatos draudimo kortelės duomenys / European Health Insurance Card Data -->
    <extension url="http://esveikata.lt/Profile/ltnhr-patient#insuranceDocument">
        <valueResource>
            <reference value="DocumentReference/123456789"/>
        </valueResource>
    </extension>

    <!-- 0..* -->
    <!-- Lietuvoje gyvenančio paciento deklaruotos gyvenamosios vietos adresas / Declared residence address of patient who lives in Lithuania -->
    <extension url="http://esveikata.lt/Profile/ltnhr-patient#addressIdentifier">

        <!-- 1..1 -->
        <!-- Paciento adreso tipas / Type of address -->
        <extension url="http://esveikata.lt/Profile/ltnhr-patient#addressIdentifier.type">
            <valueCodeableConcept>
                <coding>
                    <system value="http://esveikata.lt/classifiers/Address/Type"/>
                    <!-- Values: "declared" : deklaruotos gyvenamosios vietos adresas | "residence" : gyvenamosios vietos adresas -->
                    <code value="declared"/>
                </coding>
            </valueCodeableConcept>
        </extension>

        <!-- 1..1 -->
        <!-- Paciento deklaruotos vietos tekstinis adresas / Text representation of the address -->
        <extension url="http://esveikata.lt/Profile/ltnhr-patient#addressIdentifier.text">
            <valueString value="Vilniaus m. sav. Vilniaus m. Laisvės pr. 79"/>
        </extension>

        <!-- 1..1 -->
        <!-- Adreso identifikatorius / Address ID -->
        <extension url="http://esveikata.lt/Profile/ltnhr-patient#addressIdentifier.addressID">
            <valueIdentifier>
                <system value="http://esveikata.lt/classifiers/Address/AddressIdentifier"/>
                <value value="1827821"/>
            </valueIdentifier>
        </extension>

        <!-- 0..1 -->
        <!-- Savivaldybės identifikatorius iš adresų registro / Municipality ID -->
        <extension url="http://esveikata.lt/Profile/ltnhr-patient#addressIdentifier.municipalityID">
            <valueString value="461"/>
        </extension>

        <!-- 0..1 -->
        <!-- Vietovės identifikatorius iš adresų registro / City ID -->
        <extension url="http://esveikata.lt/Profile/ltnhr-patient#addressIdentifier.cityID">
            <valueString value="1"/>
        </extension>

        <!-- 0..1 -->
        <!-- Gatvės identifikatorius iš adresų registro / Street ID -->
        <extension url="http://esveikata.lt/Profile/ltnhr-patient#addressIdentifier.streetID">
            <valueString value="22154"/>
        </extension>

        <!-- 0..1 -->
        <!-- Namo (sklypo) numeris / House number -->
        <extension url="http://esveikata.lt/Profile/ltnhr-patient#addressIdentifier.house">
            <valueString value="5C"/>
        </extension>

        <!-- 0..1 -->
        <!-- Korpuso numeris / Hull number -->
        <extension url="http://esveikata.lt/Profile/ltnhr-patient#addressIdentifier.hull">
            <valueString value="B"/>
        </extension>

        <!-- 0..1 -->
        <!-- Buto numeris / Flat number -->
        <extension url="http://esveikata.lt/Profile/ltnhr-patient#addressIdentifier.flat">
            <valueString value="25"/>
        </extension>

        <!-- 0..1 -->
        <!-- Pašto kodas / Post code -->
        <extension url="http://esveikata.lt/Profile/ltnhr-patient#addressIdentifier.postCode">
            <valueString value="12345"/>
        </extension>
    </extension>

    <!-- 0..1 -->
    <!-- Lietuvoje gyvenančio paciento gyvenamosios vietos adresas / Residence address of patient who lives in Lithuania -->
    <extension url="http://esveikata.lt/Profile/ltnhr-patient#addressIdentifier">

        <!-- 1..1 -->
        <!-- Paciento adreso tipas / Type of address -->
        <extension url="http://esveikata.lt/Profile/ltnhr-patient#addressIdentifier.type">
            <valueCodeableConcept>
                <coding>
                    <system value="http://esveikata.lt/classifiers/Address/Type"/>
                    <!-- Values: "declared" : deklaruotos gyvenamosios vietos adresas | "residence" : gyvenamosios vietos adresas -->
                    <code value="residence"/>
                </coding>
            </valueCodeableConcept>
        </extension>

        <!-- 1..1 -->
        <!-- Paciento gyvenamosios vietos tekstinis adresas / Text representation of the address -->
        <extension url="http://esveikata.lt/Profile/ltnhr-patient#addressIdentifier.text">
            <valueString value="Vilniaus m. sav. Vilniaus m. Laisvės pr. 79"/>
        </extension>

        <!-- 0..1 -->
        <!-- Adreso identifikatorius / Address ID -->
        <extension url="http://esveikata.lt/Profile/ltnhr-patient#addressIdentifier.addressID">
            <valueIdentifier>
                <system value="http://esveikata.lt/classifiers/Address/AddressIdentifier"/>
                <value value="1827821"/>
            </valueIdentifier>
        </extension>

        <!-- 0..1 -->
        <!-- Savivaldybės identifikatorius / Municipality ID -->
        <extension url="http://esveikata.lt/Profile/ltnhr-patient#addressIdentifier.municipalityID">
            <valueString value="461"/>
        </extension>

        <!-- 0..1 -->
        <!-- Vietovės identifikatorius / City ID -->
        <extension url="http://esveikata.lt/Profile/ltnhr-patient#addressIdentifier.cityID">
            <valueString value="1"/>
        </extension>

        <!-- 0..1 -->
        <!-- Gatvės identifikatorius / Street ID -->
        <extension url="http://esveikata.lt/Profile/ltnhr-patient#addressIdentifier.streetID">
            <valueString value="22154"/>
        </extension>

        <!-- 0..1 -->
        <!-- Namo numeris / House number -->
        <extension url="http://esveikata.lt/Profile/ltnhr-patient#addressIdentifier.house">
            <valueString value="12A"/>
        </extension>

        <!-- 0..1 -->
        <!-- Korpuso numeris / Hull number -->
        <extension url="http://esveikata.lt/Profile/ltnhr-patient#addressIdentifier.hull">
            <valueString value="B"/>
        </extension>

        <!-- 0..1 -->
        <!-- Buto numeris / Flat number -->
        <extension url="http://esveikata.lt/Profile/ltnhr-patient#addressIdentifier.flat">
            <valueString value="25"/>
        </extension>

        <!-- 0..1 -->
        <!-- Pašto kodas / Post code -->
        <extension url="http://esveikata.lt/Profile/ltnhr-patient#addressIdentifier.postCode">
            <valueString value="12345"/>
        </extension>
    </extension>
    
    <!-- 0..1 -->
    <!-- Lietuvoje gyvenančio paciento virtualios dežutės adresas / Virtual P.O. box address of patient who lives in Lithuania -->
    <extension url="http://esveikata.lt/Profile/ltnhr-patient#addressIdentifier">        
        <!-- 1..1 -->
        <!-- Paciento adreso tipas / Type of address -->
        <extension url="http://esveikata.lt/Profile/ltnhr-patient#addressIdentifier.type">
            <valueCodeableConcept>
                <coding>
                    <system value="http://esveikata.lt/classifiers/Address/Type"/>
                    <code value="virtual_box"/>
                </coding>
            </valueCodeableConcept>
        </extension>
        
        <!-- 1..1 -->
        <!-- Paciento dežutės tekstinis adresas / Text representation of the address -->
        <extension url="http://esveikata.lt/Profile/ltnhr-patient#addressIdentifier.text">
            <valueString value="Vilniaus m. sav. Vilniaus m. Laisvės pr. 79"/>
        </extension>
        
        <!-- 1..1 -->
        <!-- Adreso identifikatorius / Address ID -->
        <extension url="http://esveikata.lt/Profile/ltnhr-patient#addressIdentifier.addressID">
            <valueIdentifier>
                <system value="http://esveikata.lt/classifiers/Address/AddressIdentifier"/>
                <value value="1827829"/>
            </valueIdentifier>
        </extension>       
    </extension>
    

    <!-- 0..1 -->
    <!-- Lietuvos piliečio asmens kodas / Personal Code; Pastaba: jei asmuo turi asmens kodą -->
    <!-- data-element-101 -->
    <identifier>
        <system value="http://esveikata.lt/Identifier/PersonalCode"/>
        <value value="37512092729"/>
    </identifier>

    <!-- 1..1 -->
    <!-- Unikalus paciento ESI numeris / Patient EHI No. -->
    <!-- data-element-107 -->
    <identifier>
        <system value="http://esveikata.lt/Identifier/Patient/ESI"/>
        <value value="232233"/>
    </identifier>

    <!-- 0..1 -->
    <!-- Asmens draudžiamojo identifikacinis kodas iš draudžiamųjų privalomuoju sveikatos draudimo registro / Insurance No. -->
    <identifier>
        <system value="http://esveikata.lt/Identifier/Patient/DIK"/>
        <value value="232233"/>
    </identifier>
    
    <!-- 0..1 -->
    <!-- Asmens identifikatorius SVEIDRA IS / Person identifier in SVEIDRA IS. -->
    <identifier>
        <system value="http://esveikata.lt/Identifier/Patient/SveidraID"/>
        <value value="1073765"/>
    </identifier>
    
    <!-- 0..1 -->
    <!-- Asmens E. pristatymo dėžutės duomenys / Person eDelivery address. -->
    <identifier>
        <system value="http://esveikata.lt/Identifier/eDelivery"/>
        <value value="vardpav123456"/>
    </identifier>

    <!-- 0..* -->
    <!-- data-element-110 -->
    <!-- Paciento vardo/pavardės duomenys / Patient name data -->
    <name>
        <!-- data-element-111 -->
        <!-- Paciento pavardė(-ės) / Patient surname(-s) -->
        <family value="Pavardenis"/>

        <!-- data-element-112 -->
        <!-- Paciento vardas(-ai) / Patient name(-s) -->
        <given value="Vardenis"/>
        <given value="Antravardenis"/>
        <given value="Trečiavardenis"/>
    </name>

    <!-- 0..1 -->
    <!-- Paciento telefono numeris / Telephone number -->
    <telecom>
        <system value="phone"/>
        <value value="+37067800000"/>
    </telecom>

    <!-- 0..1 -->
    <!-- Paciento elektroninio pašto adresas / E-mail address -->
    <telecom>
        <system value="email"/>
        <value value="vardenis.pavardenis@nortal.com"/>
    </telecom>

    <!-- 0..1 -->
    <!-- Paciento lytis / Patient gender-->
    <!-- Classificator: administrative-gender -->
    <gender>
        <coding>
            <system value="http://esveikata.lt/classifiers/AdministrativeGender"/>
            <code value="1"/>
            <display value="Vyras"/>
        </coding>
    </gender>

    <!-- 0..1 -->
    <!-- Paciento gimimo diena / Patient birthday -->
    <birthDate value="1984-04-02"/>

    <!-- 0..1 -->
    <!-- Paciento mirties data / Date of death -->
    <deceasedDateTime value="2014-07-08"/>
    
	<!-- 0..1 -->
    <!-- Gyvenamosios vietos adresas užsienio piliečiams / Residence address for non-LT citizens -->
    <address>
        <extension url="http://esveikata.lt/Profile/ltnhr-patient#residenceCountry">
            <valueCodeableConcept>
                <coding>
                    <system value="http://esveikata.lt/classifiers/Country"/>
                    <code value="EST"/>
                </coding>
            </valueCodeableConcept>
        </extension>
        <text value="Lõõtsa 6C, Tallinn 11415, Eesti"/>
        <line value="Lõõtsa 6C"/>
        <city value="Tallinn"/>
        <zip value="11415"/>
    </address>
	
    <!-- 0..1 -->
    <!-- Paciento šeimyninė padėtis / Patient marital status -->
    <!-- Classificator: marital-status -->
    <maritalStatus>
        <coding>
            <system value="http://hl7.org/fhir/marital-status"/>
            <code value="M"/>
            <display value="Vedęs / Ištekėjusi"/>
        </coding>
    </maritalStatus>

    <!-- 0..1 -->
    <!-- Daugiavaisio nėštumo indikatorius / Multiple pregnancy indicator -->
    <!-- Nurodoma ar pacientas yra iš daugiavaisio nėštumo bei jo gimimo eiliškumas -->
    <multipleBirthInteger value="2"/>

    <!-- 0..* -->
    <!-- Susiję asmenys / Related persons -->
    <contact>
        <!-- 1..1 -->
        <!-- Nuoroda į susijusio asmens duomenis / Reference to related person -->
        <extension url="http://esveikata.lt/Profile/ltnhr-patient#relatedPerson.reference">
            <valueResource>
                <reference value="Patient/f003/_history/1"/>
            </valueResource>
        </extension>

        <!-- 1..1 -->
        <!-- Paciento giminystės arba kito ryšio tipas (pvz., sutuoktinis, tėvas, mama). / Type of related person -->
        <!-- Classificator: related-person-type -->
        <relationship>
            <coding>
                <system value="http://esveikata.lt/classifiers/RelatedPerson/Type"/>
                <code value="1"/>
                <display value="mama"/>
            </coding>
        </relationship>

        <!-- 0..1 -->
        <!-- Susijusio asmens telefono numeris / Related person telephone number -->
        <telecom>
            <system value="phone"/>
            <value value="+37067800000"/>
        </telecom>

        <!-- 0..1 -->
        <!-- Susijusio asmens elektroninio pašto adresas / Related person e-mail address -->
        <telecom>
            <system value="email"/>
            <value value="vardenis.pavardenis@nortal.com"/>
        </telecom>
    </contact>

    <!-- 0..* -->
    <!-- Nuoroda į paciento pasirinkto sveikatos priežiūros specialisto resursą / Reference to family doctor resource -->
    <careProvider>
        <reference value="Practitioner/f003/_history/1"/>
    </careProvider>

    <!-- 0..* -->
    <!-- Sveikatos priežiūros įstaiga, prie kurios pacientas prisirašęs / Patient's nominated care provider (organization) -->
    <careProvider>

        <!-- 1..1 -->
        <!-- Laikotarpis (nuo iki datos) kiek asmuo prisirašęs prie nurodytos sveikatos priežiūros įstaigos / Period (start end) when Patient's nominated care provider  -->
        <extension url="http://esveikata.lt/Profile/ltnhr-patient#registeredPeriod">
            <valuePeriod>
                <start value="2004-02-04"/>
                <end value="2009-01-01"/>
            </valuePeriod>
        </extension>

        <!-- 1..1 -->
        <!-- Nuoroda į ASPĮ, prie kurios pacientas yra prisirašęs, resursą / Reference to Patient's nominated care provider (organization) -->
        <reference value="Organization/f003/_history/1"/>
    </careProvider>
</Patient>
