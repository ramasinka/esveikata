<?xml version="1.0" encoding="UTF-8"?>
<Patient xmlns="http://hl7.org/fhir" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.w3.org/2005/Atom http://www.hl7.org/implement/standards/fhir/patient.xsd">

    <!-- 0..1 -->
    <!-- Piliečio veido atvaizdas / Patient photo -  -->
    <extension url="http://esveikata.lt/Profile/ltnhr-patient#binaryPhoto">
        <valueResource>
            <reference value="Media/123546789"/>
        </valueResource>
    </extension>

    <!-- 0..* -->
    <!-- Asmens tapatybę patvirtinančio dokumento informacija / The identity document information -->
    <extension url="http://esveikata.lt/Profile/ltnhr-patient#identityDocument">
        <valueResource>
            <reference value="DocumentReference/123456789"/>
        </valueResource>
    </extension>

    <!-- 0..* -->
    <!-- Europos sveikatos draudimo kortelės duomenys / European Health Insurance Card Data -->
    <extension url="http://esveikata.lt/Profile/ltnhr-patient#insuranceDocument">
        <valueResource>
            <reference value="DocumentReference/123456789"/>
        </valueResource>
    </extension>

    <!-- 1..1 -->
    <!-- Unikalus paciento ESI numeris / Patient EHI No. -->
    <!-- data-element-107 -->
    <identifier>
        <system value="http://esveikata.lt/Identifier/Patient/ESI"/>
        <value value="232233"/>
    </identifier>

    <!-- 0..1 -->
    <!-- Asmens draudžiamojo identifikacinis kodas iš draudžiamųjų privalomuoju sveikatos draudimo registro / Insurance No. -->
    <identifier>
        <system value="http://esveikata.lt/Identifier/Patient/DIK"/>
        <value value="232233"/>
    </identifier>

    <!-- 0..1 -->
    <!-- Europos sveikatos draudimo kortelės arba laikinai ją pakeičiančio ES šalies narės piliečio sertifikato ar E formos pažymos nr. / E form certificate number -->
    <identifier>
        <system value="http://esveikata.lt/Identifier/Patient/eFormNumber"/>
        <value value="ABC123456"/>
    </identifier>

    <!-- 0..1 -->
    <!-- Užsienio valstybės piliečio asmens kodas arba kitas identifikavimo numeris / Foreigner personal code -->
    <identifier>
        <system value="http://esveikata.lt/Identifier/Patient/ForeignPersonalCode"/>
        <value value="987654321"/>
    </identifier>

    <!-- 0..* -->
    <!-- data-element-110 -->
    <name>
        <!-- data-element-111 -->
        <!-- Paciento pavardė(-ės) / Patient surname(-s) -->
        <family value="Pavardenis"/>

        <!-- data-element-112 -->
        <!-- Paciento vardas(-ai) / Patient name(-s) -->
        <given value="Vardenis"/>
        <given value="Antravardenis"/>
        <given value="Trečiavardenis"/>
    </name>

    <!-- 0..1 -->
    <!-- Telefono numeris / Telephone number -->
    <telecom>
        <system value="phone"/>
        <value value="+37067800000"/>
    </telecom>

    <!-- 0..1 -->
    <!-- Elektroninio pašto adresas / E-mail address -->
    <telecom>
        <system value="email"/>
        <value value="vardenis.pavardenis@nortal.com"/>
    </telecom>

    <!-- 0..1 -->
    <!-- Elektroninio pašto adresas / E-mail address -->
    <telecom>
        <extension url="http://esveikata.lt/Profile/ltnhr-patient#telecomSystem">
            <valueCodeableConcept>
                <coding>
                    <system value="edelivery"/>
                    <code value="varpav1234"/>
                </coding>
            </valueCodeableConcept>
        </extension>
        <system>
            <extension url="">
                <valueString value="edelivery"/>
            </extension>
        </system>
        <value value="varpav1234"/>
    </telecom>

    <!-- 0..1 -->
    <!-- Paciento lytis / Patient gender -->
    <gender>
        <coding>
            <system value="http://esveikata.lt/classifiers/AdministrativeGender"/>
            <code value="1"/>
            <display value="Vyras"/>
        </coding>
    </gender>

    <!-- 0..1 -->
    <!-- Paciento gimimo diena / Patient birthday -->
    <birthDate value="1984-04-02"/>

    <!-- 0..1 -->
    <!-- Paciento mirties data / Date of death -->
    <deceasedDateTime value="2014-07-08"/>

    <!-- 0..1 -->
    <!-- Gyvenamosios vietos adresas / Residence address -->
    <address>
        <extension url="http://esveikata.lt/Profile/ltnhr-patient#residenceCountry">
            <valueCodeableConcept>
                <coding>
                    <system value="http://esveikata.lt/classifiers/Country"/>
                    <code value="EST"/>
                </coding>
            </valueCodeableConcept>
        </extension>
        <text value="Lõõtsa 6C, Tallinn 11415, Eesti"/>
        <line value="Lõõtsa 6C"/>
        <city value="Tallinn"/>
        <zip value="11415"/>
    </address>

    <!-- 0..1 -->
    <!-- Paciento šeimyninė padėtis / Patient marital status -->
    <maritalStatus>
        <coding>
            <system value="http://hl7.org/fhir/marital-status"/>
            <code value="M"/>
            <display value="Vedęs / Ištekėjusi"/>
        </coding>
    </maritalStatus>

    <!-- 0..1 -->
    <!-- Daugiavaisio nėštumo indikatorius / Multiple pregnancy indicator -->
    <!-- Pastaba: Nurodoma ar pacientas yra iš daugiavaisio nėštumo bei jo gimimo eiliškumas -->
    <multipleBirthInteger value="2"/>

    <!-- 0..* -->
    <!-- Susiję asmenys / Related persons -->
    <contact>
        <!-- 0..1 -->
        <!-- Nuoroda į susijusio asmens duomenis. Privalomas jeigu nenurodyta name elemenas. / Reference to related person.  -->
        <extension url="http://esveikata.lt/Profile/ltnhr-patient#relatedPerson.reference">
            <valueResource>
                <reference value="Patient/f003/_history/1"/>
            </valueResource>
        </extension>

        <!-- 1..1 -->
        <!-- Paciento giminystės arba kito ryšio tipas (pvz., sutuoktinis, tėvas, mama). / Type of related person -->
        <relationship>
            <coding>
                <system value="http://esveikata.lt/classifiers/RelatedPerson/Type"/>
                <code value="1"/>
                <display value="Mama"/>
            </coding>
        </relationship>

        <!-- 0..1 -->
        <!-- Susijusio asmens telefono numeris / Related person telephone number -->
        <telecom>
            <system value="phone"/>
            <value value="+37067800000"/>
        </telecom>

        <!-- 0..1 -->
        <!-- Susijusio asmens elektroninio pašto adresas / Related person e-mail address -->
        <telecom>
            <system value="email"/>
            <value value="vardenis.pavardenis@nortal.com"/>
        </telecom>
    </contact>

    <!-- 0..* -->
    <!-- Susiję asmenys / Related persons -->
    <contact>

        <!-- Paciento giminystės arba kito ryšio tipas (pvz., sutuoktinis, tėvas, mama). / Type of related person -->
        <relationship>
            <coding>
                <system value="http://esveikata.lt/classifiers/RelatedPerson/Type"/>
                <code value="1"/>
                <display value="mama"/>
            </coding>
        </relationship>

        <!-- 0..1 -->
        <!-- Susijusio asmens vardas, pavardė / Related person name, surname -->
        <name>
            <!-- Susijusio asmens pavardė(-ės) / Related person surname(-s) -->
            <family value="Pavardenis"/>

            <!-- Susijusio asmens vardas(-ai) / Related person name(-s) -->
            <given value="Vardenis"/>
            <given value="Antravardenis"/>
            <given value="Trečiavardenis"/>
        </name>

        <!-- 0..1 -->
        <!-- Susijusio asmens telefono numeris / Related person telephone number -->
        <telecom>
            <system value="phone"/>
            <value value="+37067800000"/>
        </telecom>

        <!-- 0..1 -->
        <!-- Susijusio asmens elektroninio pašto adresas / Related person e-mail address -->
        <telecom>
            <system value="email"/>
            <value value="vardenis.pavardenis@nortal.com"/>
        </telecom>
    </contact>
</Patient>
