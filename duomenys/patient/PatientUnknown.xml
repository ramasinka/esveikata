<?xml version="1.0" encoding="UTF-8"?>
<!-- Neatpažinto paciento duomenys / Unknown patient -->
<Patient xmlns="http://hl7.org/fhir" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.w3.org/2005/Atom http://www.hl7.org/implement/standards/fhir/patient.xsd">

   <!-- 0..1 Nuorodą į tikrąjį pacientą / Reference to the identified patient -->
   <link>
       <!-- 1..1 Nurodą į tikrąjį pacientą, kurio duomenis turi būti naudojami vietoje šio neatpažinto paciento/ Reference to the identified patient which data should be used instead of this unknown patient -->
       <other>
           <reference value="Patient/123456879"/>
       </other>
       <!-- 1..1 Nurodos tipas (replace pagal nutylėjimą) / Link type (replace by default) -->
       <type value="replace"/>
   </link>
</Patient>