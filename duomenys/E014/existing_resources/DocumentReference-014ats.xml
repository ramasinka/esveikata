<?xml version="1.0" encoding="UTF-8"?>
<!-- 0..1 --> <!-- Privalomas, jei nurodyti ankstesni morfologiniai tyrimai (E014-ats) / Mandatory, if exist earlier morphological tests (E014-ats) -->
<!-- 9.3.4 Ankstesnio morfologinio tyrimo atsakymas / Reference to an earlier morphological test  -->
<!-- data-element-1540 -->
<DocumentReference xmlns="http://hl7.org/fhir">
    
    <!-- 1..1 -->
    <!-- Dokumento identifikatorius "Documents" integraciniame taške / Doc identifier in "Documents" endpoint -->
    <masterIdentifier>
        <system value="Documents"/>
        <value value="123456789"/>
    </masterIdentifier>
    
    <!-- 1..1 -->
    <!-- Dokumento antraštės identifikatorius "Composition" integraciniame taške / Doc identifier in "Composition" endpoint -->
    <identifier>
        <system value="Composition"/>
        <value value="1234567978"/>
    </identifier>
    
    <!-- 1..1 -->
    <!-- 10.2.4 Patologijos tyrimo Nr. / Pathology test number -->
    <identifier>
        <system value="http://esveikata.lt/Identifier/PathologyTestNumber"/>
        <value value="1234567978"/>
    </identifier>
    
    <!-- 1..1 -->
    <!-- 1.3 Pacientas, kuriam suformuotas dokumentas / Patient of document -->
    <subject>
        <reference value="Patient/32856257683256/_history/2"/>
    </subject>
    
    <!-- 1..1 -->
    <!-- Dokumento tipas E014-ats dokumentui / Document type it references to E014-ats -->
    <!-- data-element-1840 -->
	<!-- Classificator: doc-code -->
    <type>
        <coding>
            <system value="http://loinc.org"/>
            <code value="11526-1"/>
        </coding>
        
        <!-- 1..1 -->
        <!-- Dokumento pavadinimas E014-ats dokumentui / Document title for E014-ats document -->
        <text value="Patologijos tyrimo atsakymas"/>
    </type>
    
    <!-- 1..1 -->
    <!-- 1.2 Ankstesnio morfologinio tyrimo atsakymo (E014-ats) dokumentą patvirtinusio specialisto duomenys / Info about specialist who has created earlier morphological test result document -->
    <author>
        <reference value="Practitioner/12346/_history/1"/>
    </author>
    
    <!-- 1..1 -->
    <!-- 1.3 Informacija apie ankstesnio morfologinio tyrimo atsakymo (E014-ats) dokumentą pateikusią SPĮ / Info about HIS who has created this document -->
    <custodian>
        <reference value="Organization/12346/_history/1"/>
    </custodian>
    
    <!-- 1..1 -->
    <!-- Ankstesnio morfologinio tyrimo atsakymo (E014-ats) dokumento sukūrimo data / Document creation date -->
    <created value="2005-12-24T09:43:41+11:00"/>
    
    <!-- 1..1 -->
    <!-- Data, kada sukurta dokumento nuoroda / When this reference was created -->
    <indexed value="2005-12-24T09:43:41+11:00"/>
    
    <!-- 1..1 --> <!-- Nenaudojamas laukas, tačiau privalomas / Not used, but mandatory --> 
    <!-- Nuorodos būsena / The status of this document reference -->
    <!-- Values: "current" (by default) -->
	<!-- Classificator: document-reference-status -->
    <status value="current"/>
    
    <!-- 1..1 -->
    <!-- Medicininio dokumento būsena iš kompozicijos būsenų + signed pasirašytam dokumentui / Medical document state derived from Composition states + signed status for signed docs -->
	<!-- Classificator: document-status -->
    <docStatus>
        <coding>
            <system value="http://esveikata.lt/classifiers/Document/Status"/>
            <code value="signed"/>
        </coding>
    </docStatus>
    
    <!-- 1..1 -->
    <!-- Dokumento mime tipas / The mime type of the source document. Currently unknown so text/plain by default -->
    <mimeType value="text/xml"/>
</DocumentReference>
