<?xml version="1.0" encoding="UTF-8"?>
<!-- 0..1 -->
<!-- Privalomas, jei yra bent viena susijusi diagnozė / Mandatory if exist at least one related diagnosis -->
<!-- 4.3.5 Susijusi diagnozė / Related Diagnosis -->
<!-- data-element-624 -->
<Condition xmlns="http://hl7.org/fhir">

    <!-- 1..1 -->
    <!-- Nuoroda į pacientą, kuriam buvo nustatyta ši diagnozė / Patient who the condition record is associated with -->
    
    <subject>
        <reference value="Patient/32856257683256/_history/2"/>
    </subject>

    <!-- 1..1 -->
    <!-- 4.3.5.1 - 4.3.5.2 Diagnozės kodas ir pavadinimas / Diagnosis code and description -->
    <!-- data-element-1932 -->
    
    <code>
        <coding>
            <!-- 1..1 -->
            <!-- 4.3.5.1 Diagnozės TLK-10-AM kodas / Diagnosis TLK-10-AM code -->
            <!-- Classificator: tlk-10-am -->
            <system value="http://esveikata.lt/classifiers/TLK-10-AM"/>
            <code value="493"/>
            <!-- 4.3.5.2 Diagnozės pavadinimas / Name -->
            <display value="Asthma"/>
        </coding>
    </code>

    <!-- 1..1 -->
    <!-- Nenaudojamas laukas, tačiau privalomas / Not used, but mandatory -->
    <!-- Diagnozės būsena / The clinical status of the condition -->
    <!-- Values: confirmed (by default) -->
	<!-- Classificator: condition-status -->
    <status value="confirmed"/>

    <!-- 1..0 -->
    <!-- 4.3.5.3 Susijusios diagnozės aprašymas / Related diagnosis description -->
    <notes value="Related diagnosis description"/>
</Condition>
