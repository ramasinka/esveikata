<Organization xmlns="http://hl7.org/fhir" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.w3.org/2005/Atom http://www.hl7.org/implement/standards/fhir/organization.xsd">
    
    <!-- 0..* -->
    <!-- Įstaigos registracijos adreso informacija / Address of registration information -->
    <extension url="http://esveikata.lt/Profile/ltnhr-organization#addressIdentifier">
        
        <!-- 1..1 -->
        <!-- Įstaigos adreso tipas / Type of address -->
        <!-- Values: "activity" : juridinio asmens arba padalinio veiklos adresas | "registration" : juridinio asmens registracijos adresas -->
        <!-- Classificator: organization-type -->
        <extension url="http://esveikata.lt/Profile/ltnhr-organization#addressIdentifier.type">
            <valueCodeableConcept>
                <coding>
                    <system value="http://esveikata.lt/classifiers/AddressIdentifier/Type"/>
                    <code value = "registration"/>
                </coding>
            </valueCodeableConcept>
        </extension>
        
        <!-- 1..1 -->
        <!-- Įstaigos tekstinis adresas / Text representation of the address -->
        <extension url="http://esveikata.lt/Profile/ltnhr-organization#addressIdentifier.text">
            <valueString value="Vilniaus m. sav. Vilniaus m. Laisvės pr. 79" />
        </extension>
        
        <!-- 1..1 -->
        <!-- Adreso identifikatorius / Address ID -->
        <extension url="http://esveikata.lt/Profile/ltnhr-organization#addressIdentifier.addressID">
            <valueCodeableConcept>
                <coding>
                    <system value="http://esveikata.lt/classifiers/AddressIdentifier/AddressID"/>
                    <code value = "1827111"/>
                </coding>
            </valueCodeableConcept>
        </extension>
        
        <!-- 0..1 -->		
        <!-- Savivaldybės identifikatorius / Municipality ID -->
        <extension url="http://esveikata.lt/Profile/ltnhr-organization#addressIdentifier.municipalityID">
            <valueCodeableConcept>
                <coding>
                    <system value="http://esveikata.lt/classifiers/AddressIdentifier/MunicipalityID"/>
                    <code value = "460"/>
                </coding>
            </valueCodeableConcept>
        </extension>
        
        <!-- 0..1 -->
        <!-- Vietovės identifikatorius / City ID -->
        <extension url="http://esveikata.lt/Profile/ltnhr-organization#addressIdentifier.cityID">
            <valueCodeableConcept>
                <coding>
                    <system value = "http://esveikata.lt/classifiers/AddressIdentifier/CityID"/>
                    <code value = "1"/>
                </coding>
            </valueCodeableConcept>
        </extension>
        
        <!-- 0..1 -->
        <!-- Gatvės identifikatorius / Street ID -->
        <extension url="http://esveikata.lt/Profile/ltnhr-organization#addressIdentifier.streetID">
            <valueCodeableConcept>
                <coding>
                    <system value = "http://esveikata.lt/classifiers/AddressIdentifier/StreetID"/>
                    <code value = "22151"/>
                </coding>
            </valueCodeableConcept>
        </extension>
        
        <!-- 1..1 -->
        <!-- Namo numeris / House number -->
        <extension url="http://esveikata.lt/Profile/ltnhr-organization#addressIdentifier.house">
            <valueString value="54C"/>
        </extension>
        
        <!-- 0..1 -->
        <!-- Korpuso numeris / Hull number -->
        <extension url="http://esveikata.lt/Profile/ltnhr-organization#addressIdentifier.hull">
            <valueString value="A"/>
        </extension>
        
        <!-- 0..1 -->
        <!-- Buto numeris / Flat number -->
        <extension url="http://esveikata.lt/Profile/ltnhr-organization#addressIdentifier.flat">
            <valueString value="1"/>
        </extension>
    </extension>
    
    <!-- 0..* -->
    <!-- Įstaigos veiklos adreso informacija / Address of activity information -->
    <extension url="http://esveikata.lt/Profile/ltnhr-organization#addressIdentifier">
        
        <!-- 1..1 -->
        <!-- Įstaigos adreso tipas / Type of address -->
        <!-- Values: "activity" : juridinio asmens arba padalinio veiklos adresas | "registration" : juridinio asmens registracijos adresas -->
        <!-- Classificator: address-identifier-type -->
        <extension url="http://esveikata.lt/Profile/ltnhr-organization#addressIdentifier.type">
            <valueCodeableConcept>
                <coding>
                    <system value="http://esveikata.lt/classifiers/AddressIdentifier/Type"/>
                    <code value = "activity"/>
                </coding>
            </valueCodeableConcept>
        </extension>
        
        <!-- 1..1 -->
        <!-- Įstaigos tekstinis adresas / Text representation of the address -->
        <extension url="http://esveikata.lt/Profile/ltnhr-organization#addressIdentifier.text">
            <valueString value="Vilniaus m. sav. Vilniaus m. Laisvės pr. 79" />
        </extension>
        
        <!-- 1..1 -->
        <!-- Adreso identifikatorius / Address ID -->
        <extension url="http://esveikata.lt/Profile/ltnhr-organization#addressIdentifier.addressID">
            <valueCodeableConcept>
                <coding>
                    <system value="http://esveikata.lt/classifiers/AddressIdentifier/AddressID"/>
                    <code value = "1827111"/>
                </coding>
            </valueCodeableConcept>
        </extension>
        
        <!-- 0..1 -->
        <!-- Savivaldybės identifikatorius / Municipality ID -->
        <extension url="http://esveikata.lt/Profile/ltnhr-organization#addressIdentifier.municipalityID">
            <valueCodeableConcept>
                <coding>
                    <system value="http://esveikata.lt/classifiers/AddressIdentifier/MunicipalityID"/>
                    <code value = "460"/>
                </coding>
            </valueCodeableConcept>
        </extension>
        
        <!-- 0..1 -->
        <!-- Vietovės identifikatorius / City ID -->
        <extension url="http://esveikata.lt/Profile/ltnhr-organization#addressIdentifier.cityID">
            <valueCodeableConcept>
                <coding>
                    <system value = "http://esveikata.lt/classifiers/AddressIdentifier/CityID"/>
                    <code value = "1"/>
                </coding>
            </valueCodeableConcept>
        </extension>
        
        <!-- 0..1 -->
        <!-- Gatvės identifikatorius / Street ID -->
        <extension url="http://esveikata.lt/Profile/ltnhr-organization#addressIdentifier.streetID">
            <valueCodeableConcept>
                <coding>
                    <system value = "http://esveikata.lt/classifiers/AddressIdentifier/StreetID"/>
                    <code value = "22151"/>
                </coding>
            </valueCodeableConcept>
        </extension>
        
        <!-- 1..1 -->
        <!-- Namo numeris / House number -->
        <extension url="http://esveikata.lt/Profile/ltnhr-organization#addressIdentifier.house">
            <valueString value="54C"/>
        </extension>
        
        <!-- 0..1 -->
        <!-- Korpuso numeris / Hull number -->
        <extension url="http://esveikata.lt/Profile/ltnhr-organization#addressIdentifier.hull">
            <valueString value="C"/>
        </extension>
        
        <!-- 0..1 -->
        <!-- Buto numeris / Flat number -->
        <extension url="http://esveikata.lt/Profile/ltnhr-organization#addressIdentifier.flat">
            <valueString value="2"/>
        </extension>
    </extension>
    
    <!-- 0..1 -->
    <!-- Sveikatinimo įstaigą administruojančios teritorinės ligonių kasos JAR kodas / JAR code of insurance organization which administrates this organization. -->
    <extension url="http://esveikata.lt/Profile/ltnhr-organization#insuranceAdministrator">
        <valueCodeableConcept>
            <coding>
                <system value = "http://esveikata.lt/Identifier/JAR"/>
                <code value = "17777767890"/>
            </coding>
        </valueCodeableConcept>
    </extension>
    
    <!-- 0..1 -->
    <!-- Sveikatinimo įstaigos kodas (JAR) / Registration Code -->
    <!-- data-element-280  -->
    <identifier>
        <system value="http://esveikata.lt/Identifier/JAR" />
        <value value="188899999" />
    </identifier>
    
    <!-- 0..1 -->
    <!-- Sveikatos priežiūros organizacinio vieneto (įstaigos, jos filialo, skyriaus, filialo skyriaus ir pan.) kodas SVEIDRA IS sistemoje / SVEIDRA ID issued by National health insurance fund -->
    <!-- data-element-297 -->
    <identifier>
        <system value="http://esveikata.lt/Identifier/SVEIDRAID"/>
        <value value="4688"/>
    </identifier>
    
    <!-- 0..1 -->
    <!-- Sveikatos priežiūros įstaigos pavadinimas / HCP name -->
    <!-- data-element-270 -->
    <name value="Miesto poliklinika" />
    
    <!-- 1..1 -->
    <!-- Organizacijos tipas: pagrindinis sveikatos priežiūros paslaugų teikėjas (visa ligoninė) arba jos skyrius / Type of organization: Main Healthcare Provider (whole Hospital) or its department -->
	<!-- Classificator: organization-type -->
    <type>
        <coding>
            <system value="http://hl7.org/fhir/organization-type"/>
            <!-- Values: "prov" : Sveikatos priežiūros paslaugų teikėjas / Healthcare Provider | "dept" - Ligoninės skyrius / Hospital Department -->
            <code value="prov"/>
        </coding>
    </type>
    
    <!-- 0..1 -->
    <!-- Sveikatinimo įstaigos telefono numeris / HCP telephone number -->
    <telecom>
        <system value="phone"/>
        <value value="+37067800000"/>
    </telecom>
    
    <!-- 0..1 -->
    <!-- Sveikatinimo įstaigos telefono numeris / HCP telephone number -->
    <telecom>
        <system value="phone"/>
        <value value="+37067811111"/>
        <use value="mobile"/>
    </telecom>
    
    <!-- 0..1 -->
    <!-- Sveikatinimo įstaigos fakso numeris / HCP Fax number -->
    <telecom>
        <system value="fax"/>
        <value value="+37067822222"/>
    </telecom>
    
    <!-- 0..1 -->
    <!-- Sveikatinimo įstaigos el. pašto adresas / HCP email -->
    <telecom>
        <system value="email"/>
        <value value="organization@domain.lt"/>
    </telecom>
    
    <!-- 0..1 -->
    <!-- Tuo atveju jei ligoninės skyrius, nuoroda į tėvinį skyrių ar sveikatinimo organizaciją / In case of department, reference to parent department or health organization -->
    <partOf>
		<reference value="Organization/123/_history/1"/>
	</partOf>
</Organization>