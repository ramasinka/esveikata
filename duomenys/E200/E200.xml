    <entry>
        <id>cid:2</id>
        <content type="text/xml">
            <!-- 1..1 -->
            <!-- 7.1 Dokumento antraštės informacija "Composition" resurse / Document header information in "Composition" resource -->
            <Composition xmlns="http://hl7.org/fhir">

                <!-- 0..1 -->
                <!-- 1.2.4. Skyrius, kuriame dirba dokumento autorius (specialistas) / Department where specialist works -->
                <extension url="http://esveikata.lt/Profile/ltnhr-composition#authorDepartment">
                    <valueResource>
                        <reference value="Organization/123546789"/>
                    </valueResource>
                </extension>

                <!-- 1..1 -->
                <!-- 1.2.5. Gydytojo profesinė kvalifikacija / Health Care Specialist‘s Qualification -->
                <!-- data-element-250 -->
                
				<!-- Classificator: qualification-code -->
                <extension url="http://esveikata.lt/Profile/ltnhr-composition#authorQualification">
                    <valueCodeableConcept>
                        <coding>
                            <system value="http://esveikata.lt/classifiers/QualificationCode"/>
                            <code value="221227"/>
                            <display value="Gydytojas neurologas"/>
                        </coding>
                    </valueCodeableConcept>
                </extension>

                <!-- 1..1 -->
                <!-- Dokumento paruošimo data ir laikas / Composition editing time. The time this document was created -->
                <date value="2013-02-01T12:30:02"/>

                <!-- 1..1 -->
                <!-- CodeableConcept Kind of composition. What type of document is this - eg Discharge Summary, referral, Consultation note -->
                <type>
                    <coding>
                        <!-- 1..1 -->
                        <!-- Dokumento tipas E200 dokumentui / Document type by loinc for E200 document -->
                        <!-- Classificator: doc-code -->
                        <system value="http://loinc.org"/>
                        <code value="X-LAB-ORDER"/>
                    </coding>

                    <!-- 1..1 -->
                    <!-- Dokumento pavadinimas E200 dokumentui / Document title for E200 document -->
                    <text value="Laboratorinio tyrimo užsakymas"/>
                </type>

                <!-- 1..1  -->
                <!-- Dokumento būsena / Status of the document -->
                <!-- Values: Dalinai pateiktam dokumentui : "preliminary" | paruoštam pasirašymui : "final" | pakeistam po pasirašymo : "amended" -->
                
				<!-- Classificator: composition-status -->
                <status value="final"/>

                <!-- 1..1 -->
                <!-- Dokumento konfidencialumo lygis / Coding as defined by affinity domain -->
                <!-- Classificator: confidentiality -->
                <confidentiality>
                    <system value="http://esveikata.lt/classifiers/Confidentiality"/>
                    <code value="N"/>
                </confidentiality>

                <!-- 1..1 -->
                <!-- 1.3 Informacija apie pacientą / Information about the patient -->
                <!-- data-element-1 -->
                
                <subject>
                    <reference value="Patient/32856257683256/_history/2"/>
                </subject>

                <!-- 1..1 -->
                <!-- 1.2 Informacija apie sveikatos priežiūros specialistą / Information about Health Care Specialist -->
                <!-- data-element-2 -->
                
                <author>
                    <reference value="Practitioner/222/_history/1"/>
                </author>

                <!-- 1..1 -->
                <!-- 1.1 Informacija apie asmens sveikatos priežiūros įstaigą / Organization who is the author of the document -->
                
                <custodian>
                    <reference value="Organization/222/_history/1"/>
                </custodian>
                
                <!-- 1..1 -->
                <!-- Įvykis, kurio pagrindu yra kuriamas dokumentas / The event being documented. -->
                <!-- data-element-3 -->
				<!-- Classificator: event-type -->
                <event>
                    <code>
                        <coding>
                            <system value="http://esveikata.lt/classifiers/EventType"/>
                            <code value="3"/>
                            <display value="Laboratorinio tyrimo užsakymo (siuntimo) sukūrimas"/>
                        </coding>
                    </code>
                    <detail>
                        <reference value="cid:1"/>
                    </detail>
                </event>

                <!-- 1..1 -->
                <!-- Paciento atvykimo duomenys / Patient Reception Data -->
                <!-- data-element-19 -->
                
                <encounter>
                    <reference value="Encounter/38765/_history/1"/>
                </encounter>

                <!-- 1..1 -->
                <!-- 7.2 Informacija apie laboratorinį ėminį / Information on the test substance -->
                <section>
                    <code>
                        <coding>
                            <!-- 1..1 -->
                            <!-- Classificator: doc-section-code -->
                            <system value="http://loinc.org"/>
                            <code value="57139-8"/>
                        </coding>
                    </code>

                    <content>
                        <!-- 1..1 -->
                        <!-- Nuoroda į skyrimo duomenis, kurie buvo sukurti E025/E027. Laboratorinio ėminio informacija pasiekiama Order.details->DiagnosticOrder.item.specimen / The Order which has been created during E025/E027 form submission. Specimen data is accessible as Order.details->DiagnosticOrder.item.specimen -->
                        <reference value="Order/123/_history/1"/>
                    </content>
                </section>
                
                <!-- 1..1 -->
                <!-- Skyrimo (Order) būsenos pakeitimas / Order status changing OrderResponse -->
                <section>
                    <code>
                        <coding>
                            <!-- 1..1 -->
                            <!-- Classificator: doc-section-code -->
                            <system value="http://loinc.org"/>
                            <code value="65842-7"/>
                        </coding>
                    </code>
                    
                    <content>
                        <!-- 1..1 -->
                        <!-- Skiepo skyrimo būseną keičiantis OrderResponse resursas / Order status changing OrderResponse -->
                        <reference value="OrderResponse/19/_history/1"/>
                    </content>
                </section>
            </Composition>
        </content>
    </entry>

    <!-- 1..1 -->
    <!-- Paskyrimo/Užsakymo duomenys / Order data -->
    <!-- Resursas jau egzistuoja ESPBI sistemoje (pateiktas su E025 dokumentu), šioje tranzakcijoje jis atnaujinamas (atnaujinama nuoroda į DiagnosticOrder resurso versiją) -->
    <entry>
        <id>Order/123</id>
        <link href="Order/123/_history/1" rel="self"/>
        <content type="text/xml">
            <Order xmlns="http://hl7.org/fhir">

                <!-- 1..1 -->
                <!-- Skyrimo tipas / Order type  -->
				<!-- Classificator: order-type -->
                <extension url="http://esveikata.lt/Profile/ltnhr-order#type">
                    <valueCodeableConcept>
                        <coding>
                            <system value="http://esveikata.lt/classifiers/OrderType"/>
                            <!-- Values: "2" : Laboratorinio tyrimo skyrimas -->
                            <code value="2"/>
                            <display value="Laboratorinio tyrimo užsakymo (siuntimo) skyrimas"/>
                        </coding>
                    </valueCodeableConcept>
                </extension>
                
                <!-- 1..1 -->
                <!-- Sveikatos priežiūros specialisto, sukūrusio siuntimą, profesinė kvalifikacija / Health Care Specialist‘s who made this order Qualification -->
                <extension url="http://esveikata.lt/Profile/ltnhr-order#sourceQualification">
                    <valueCodeableConcept>
                        <!-- 1..1 -->
                        <!-- Specialisto prof. kvalifikacijos kodas / Target specialist prof qualification code -->
                        <coding>
                            <system value="http://esveikata.lt/classifiers/QualificationCode" />
                            <code value="221227" />
                            <display value="Gydytojas neurologas" />
                        </coding>
                    </valueCodeableConcept>
                </extension>

                <!-- 0..* -->
                <!-- Sveikatos priežiūros specialisto, sukūrusio siuntimą, skyrius / Department where specialists works -->
                <extension url="http://esveikata.lt/Profile/ltnhr-order#sourceDepartment">
                    <valueResource>
                        <reference value="Organization/123546"/>
                    </valueResource>
                </extension>
                
                <!-- 1..1 -->
                <!-- Tiriamos medžiagos tipas ir aprašymas. Ėminio patikslinimas įvedamas į text lauką. -->
                <extension url="http://esveikata.lt/Profile/ltnhr-order#specimenType">
                    <valueCodeableConcept>
                        <coding>
                            <!-- 1..1 -->
                            <!-- Tiriamos medžiagos tipas / Kind of material that forms the specimen -->
                            <!-- Classificator: D.4.47. Ėminių klasifikatorius -->
                            <system value="http://esveikata.lt/classifiers/Specimen/Type"/>
                            <code value="123456"/>
                            <display value="Tiriamos medžiagos tipas"/>
                        </coding>
                        
                        <!-- 0..1 -->
                        <!-- Privalomas, jei „Ėminys“ nurodyta „Kita“ / Mandatory, if spacemen value is "Other" -->
                        <!-- Ėminio patikslinimas  / Specimen description -->
                        <text value="Left hand blood"/>
                    </valueCodeableConcept>
                </extension>
                
                <!-- 1..1 -->
                <!-- Pageidaujamo tyrimo ar tyrimo skyrimo kodas (coding laukas). -->
                <!-- Classificator: diagnostic-type -->
                <extension url="http://esveikata.lt/Profile/ltnhr-order#diagnosticItemCode">
                    <valueCodeableConcept>
                        <coding>
                            <system value="http://esveikata.lt/classifiers/DiagnosticOrder/ItemCode"/>
                            <code value="123456"/>
                            <!-- 0..1 -->
                            <!-- Tyrimo pavadinimas / Test name  -->
                            <display value="Laboratorinio tyrimo kažkoks tyrimas"/>
                        </coding>
                    </valueCodeableConcept>
                </extension>

                <!-- 1..1 -->
                <!-- 7.2.1	Užsakymo data / Order issue date -->
                <!-- data-element-1210 -->
                <date value="2014-02-21T09:03:04+11:00"/>
                
                <!-- 1..1 -->
                <!-- Nuoroda į paciento, kuriam sukurtas skiepo siuntimas, resursą / Patient who has been ordered -->
                <!-- data-element-1 -->
                <subject>
                    <reference value="Patient/32856257683256/_history/2"/>
                </subject>
                
                <!-- 1..1 -->
                <!-- Nuoroda į sveikatos priežiūros specialisto, kuris sukūrė siuntimą, resursą / Health care specialist who made this order -->
                <!-- data-element-20 -->
                <source>
                    <reference value="Practitioner/987654321/_history/1"/>
                </source>
                
                <!-- 1..1 -->
                <!-- Atvykimo duomenys, kurių pagrindu buvo sukurtas šis skyrimo resursas / Encounter where this Order was created at -->
                <detail>
                    <reference value="Encounter/1000017570/_history/1000018634"/>
                </detail>
                
                <!-- 1..1 -->
                <!-- Detalūs tyrimo duomenys ir būsena / Detailed info about what is ordered and its status -->
                <detail>
                    <reference value="cid:3"/>
                </detail>
            </Order>
        </content>
    </entry>

    <!-- 1..1 -->
    <!-- Tyrimo duomenys / DiagnosticOrder data -->
    <entry>
        <id>cid:3</id>
        <content type="text/xml">
            <DiagnosticOrder xmlns="http://hl7.org/fhir">

                <!-- 1..1 -->
                <!-- 1.3 Nuoroda į pacientą, kuriam yra atliekamas tyrimas / Reference to the patient -->
                <subject>
                    <reference value="Patient/32856257683256/_history/2"/>
                </subject>

                <!-- 1..1 -->
                <!-- Nuoroda į specialistą, kuris užsakė tyrimą (šio E200 dokumento autorius) / Reference to specialist who ordered the DiagnosticOrder (author of this E200 doc) -->
                <orderer>
                    <reference value="Practitioner/222/_history/1"/>
                </orderer>

                <!-- 1..1 -->
                <!-- 7.2.5 Papildomas aprašymas laboratorijai / Additional Details for the Laboratory -->
                <!-- data-element-1250 -->
                <clinicalNotes value="Additional Details for the Laboratory"/>
                
                <!-- 1..1 -->
                <!-- Tyrimo užsakymo būsena -->
                <status value="requested"/>

                <item>
                    <code>
                        <!-- 1..1 -->
                        <!-- Tyrimų pavadinimų klasifikatorius / Test name classificator -->
                        <!-- Classificator: D.4.48. Tyrimų pavadinimų klasifikatorius -->
                        
						<!-- Classificator: diagnostic-type -->
                        <coding>
                            <system value="http://esveikata.lt/classifiers/DiagnosticOrder/ItemCode"/>
                            <code value="123546789"/>
                            <display value="Lipid Panel"/>
                        </coding>
                    </code>

                    <!-- 1..1 -->
                    <!-- 7.2.2	Tiriamasis ėminys arba tiriamoji medžiaga / Specimen data -->
                    <!-- data-element-1220 -->
                    <specimen>
                        <reference value="cid:4"/>
                    </specimen>
                </item>
            </DiagnosticOrder>
        </content>
    </entry>

    <!-- 1..1 -->
    <!-- 7.2.2 Tiriamasis ėminys arba tiriamoji medžiaga / Specimen data -->
    <entry>
        <id>cid:4</id>
        <content type="text/xml">
            <Specimen xmlns="http://hl7.org/fhir">

                <!-- 1..* -->
                <!-- 7.2.2.2 Ėminio registracijos kodas / Specimen code -->
                <!-- data-element-1222 -->
                <identifier>
                    <system value="http://esveikata.lt/classifiers/Specimen/RegistrationCode"/>
                    <value value="123456789"/>
                </identifier>

                <!-- 1..1 -->
                <!-- 7.2.2.1 Ėminys / Specimen-->
                <!-- data-element-1221 -->
                <type>
                    <coding>
                        <!-- 1..1 -->
                        <!-- 7.2.2.1 Tiriamos medžiagos tipas / Kind of material that forms the specimen -->
                        
						<!-- Classificator: specimen-type -->
                        <system value="http://esveikata.lt/classifiers/Specimen/Type"/>
                        <code value="123456"/>

                        <!-- 0..1 -->
                        <display value="Venous blood specimen"/>
                    </coding>

                    <!-- 0..1 -->
                    <!-- Privalomas, jei „Ėminys“ nurodyta „Kita“ / Mandatory, if spacemen value is Other -->
                    <!-- Ėminio patikslinimas / Specimen description -->
                    
                    <text value="Left hand blood"/>
                </type>

                <!-- 1..1 -->
                <!-- Nuoroda į paciento, kurio tiriamasis ėminys, duomenis / Reference to the patient whose specimen data -->
                
                <subject>
                    <reference value="Patient/32856257683256/_history/2"/>
                </subject>

                <collection>
                    <!-- 1..1 -->
                    <!-- 7.2.4 Ėminio paėmimo data ir laikas / Date and time when specimen was taken -->
                    <!-- data-element-1230 -->
                    <collectedDateTime value="2011-05-30T06:15:00"/>
                </collection>

                <container>
                    <!-- 1..1 -->
                    <!-- 7.2.3	Ėminio laikymo, gabenimo informacija / Specimen holding/transportation details -->
                    <!-- data-element-1223 -->
                    <extension url="http://esveikata.lt/Profile/ltnhr-specimen#storageTransportationDetails">
                        <valueCodeableConcept>

                            <coding>
                                <!-- 1..1 -->
                                <!-- Classificator: specimen-storage-trans-detail -->
                                <system value="http://esveikata.lt/classifiers/Specimen/StorageTransportationDetails"/>
                                <!-- Values: "1" : 7.2.3.1 Kambario temperatūroje/At room temperature | "2" : 7.2.3.2 Šaldytuve/Refrigerator | "3" : 7.2.3.3 Kita/Other -->
                                <code value="3"/>
                                <!-- 0..1 -->
                                <display value="Kita"/>
                            </coding>

                            <!--0..1 -->
                            <!-- Privalomas, jei „Ėminio laikymo, gabenimo informacija“ nurodyta „Kita“ / Mandatory, if "Specimen holding/transportation details" value "Other" (code "3") -->
                            <!-- 7.2.3.3. Kita / Other -->
                            <text value="+1 laipsnis temperaturoje"/>
                        </valueCodeableConcept>
                    </extension>
                </container>
            </Specimen>
        </content>
    </entry>
    
    <!-- 0..1 -->
    <!-- Skyrimo atsakas keičiantis skyrimo būseną į priimtas (accepted)/ An OrderResponse for changing the status of the order to accepted -->
    <entry>
        <id>OrderResponse/19</id>
        <link href="OrderResponse/19/_history/1" rel="self"/>
        <content type="text/xml">
            <OrderResponse xmlns="http://hl7.org/fhir" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.w3.org/2005/Atom http://www.hl7.org/implement/standards/fhir/orderresponse.xsd">
                
                <!-- 1..1 -->
                <!-- Nuoroda į skyrimo duomenis (Order resursas), kurių būseną aprašo šis OrderResponse resursas / The order that this is a response to -->
                <request>
                    <reference value="Order/123/_history/1"/>
                </request>
                
                <!-- 1..1 -->
                <!-- Skyrimo būsenos pakeitimo data / When the response was made -->
                <date value="2013-02-01T12:30:02"/>
                
                <!-- Skyrimo būseną pakeitęs specialistas/ Who made the response -->
                <who>
                    <reference value="Practitioner/222/_history/1"/>
                </who>
                
                <!-- Naują skyrimo būseną atliktam skyrimui / A new order status -->
                <code value="accepted"/>
            </OrderResponse>
        </content>
    </entry>
