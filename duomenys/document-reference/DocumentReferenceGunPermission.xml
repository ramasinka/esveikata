<?xml version="1.0" encoding="UTF-8"?>
<!-- Leidimas laikyti ar nešiotis ginklą / Gun permission document -->
<DocumentReference xmlns="http://hl7.org/fhir" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.w3.org/2005/Atom http://www.hl7.org/implement/standards/fhir/documentreference.xsd">

    <!-- 1..1 -->
    <!-- Leidimo laikyti ar nešiotis ginklą numeris / Document number -->
    <masterIdentifier>
        <system value="http://esveikata.lt/Identifier/gunPermissionNumber"/>
        <value value="ABC123456"/>
    </masterIdentifier>

    <!-- 1..1 -->
    <!-- Nuoroda į paciento, kuriam priklauso leidimas laikyti ar nešiotis ginklą, resursą / The owner of gun permission document -->
    <subject>
        <reference value="Patient/123456"/>
    </subject>

    <!-- 1..1 -->
    <!-- Asmens tapatybę patvirtinančio dokumento rūšis / The identity document type -->
    <!-- Classificator: identity-document-type -->
    <type>
        <coding>
            <system value="http://esveikata.lt/classifiers/IdentityDocument/Type"/>
            <code value="LG"/>
            <display value="Leidimas laikyti ar nešiotis ginklą"/>
        </coding>
    </type>

    <!-- 1..1 -->
    <!-- Dokumentą užregistravęs specialistas /The practitioner registered insurance document /  -->
    <author>
        <reference value="Practitioner/12346/_history/1"/>
    </author>

    <!-- 1..1 -->
    <!-- Data, kada buvo sukurta ši nuoroda / When this reference was created -->
    <indexed value="2005-12-24T09:43:41+11:00"/>

    <!-- 1..1 --> <!-- Nenaudojamas laukas, tačiau privalomas / Not used, but mandatory --> 
    <!-- Nuorodos būsena / The status of this document reference -->
    <!-- Values: "current" (by default) -->
    <status value="current"/>

    <!-- 1..1 -->
    <!-- The mime type of the source document -->
    <!-- Values: "text/plain" (by default) -->
    <mimeType value="text/plain"/>

    <context>
        <period>
            <!-- 1..1 -->
            <!-- Leidimo laikyti ar nešiotis ginklą galiojimo panaikinimo data / Gun permission document expiration Date-->
            <end value="2004-12-23T08:01:00"/>
        </period>
    </context>
</DocumentReference>