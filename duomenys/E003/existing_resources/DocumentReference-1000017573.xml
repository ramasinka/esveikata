<?xml version="1.0" encoding="UTF-8"?>
<!-- 0..1 -->
<!-- Privalomas, jei yra nors viena išduota pažyma / Mandatory if exist at least one certificate -->
<!-- 3.4.2 Išduota pažyma / Certificate given -->
<DocumentReference xmlns="http://hl7.org/fhir">
    
    <!-- 1..1 -->
    <!-- 3.4.2.2 Išduotos pažymos numeris / Certificate of Incapacity No. -->
    <!-- data-element-1810 -->
    <masterIdentifier>
        <use value="temp"/>
        <system value="http://esveikata.lt/Identifier/Document"/>
        <value value="123456789"/>
    </masterIdentifier>
    
    <!-- 1..1 -->
    <!-- Pacientas, kuriam išduota pažyma / Patient of certificate -->
    <subject>
        <reference value="Patient/1000017309/_history/1000018374"/>
    </subject>
    
    <!-- 1..1 -->
    <!-- 3.4.2.2 Dokumento tipas / Document type it references to -->
    <!-- data-element-1840 -->
	<!-- Classificator: document-type -->
    <type>
        <coding>
            <system value="http://esveikata.lt/classifiers/Document/Type"/>
            <code value="medicinine_pazyma"/>
        </coding>
    </type>
    
    <!-- 1..1 -->
    <!-- 3.4.2.2 Pažymėjimą išrašęs specialistas / The author of certificate -->
    <author>
        <reference value="Practitioner/1000015885/_history/1000016940"/>
    </author>
    
    <!-- 1..1 -->
    <!-- 3.4.2.2 Data, kada buvo sukurta ši nuoroda / When this reference was created -->
    <indexed value="2014-10-27T19:43:41+03:00"/>
    
    <!-- 1..1 --> <!-- Nenaudojamas laukas, tačiau privalomas / Not used, but mandatory --> 
    <!-- Nuorodos būsena / The status of this document reference -->
    <!-- Values: "current" (by default) -->
	<!-- Classificator: document-reference-status -->
    <status value="current"/>
    
    <!-- 1..1 --> <!-- Nenaudojamas laukas, tačiau privalomas / Not used, but mandatory --> 
    <!-- The mime type of the source document -->
    <!-- Values: "text/plain" (by default) -->
    <mimeType value="text/xml"/>
    
    <context>
        <period>
            <!-- 1..1 -->
            <!-- Nedarbingumo (nedarbingumo pažymėjimo) pradžios data / Incapacity Start Date -->
            <!-- data-element-1820 -->
            <start value="2014-10-27T19:43:41"/>
            
            <!-- 1..1 -->
            <!-- Nedarbingumo (nedarbingumo pažymėjimo) pabaigos data / Incapacity End Date-->
            <!-- data-element-1830 -->
            <end value="2014-10-30T19:43:41"/>
        </period>
    </context>
</DocumentReference>
