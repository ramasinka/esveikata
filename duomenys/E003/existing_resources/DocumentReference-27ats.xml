<?xml version="1.0" encoding="UTF-8"?>
<!-- 0..1 -->
<!-- Privalomas, jei yra nors vienas laboratorinių ir instrumentinių tyrimų rezultatų dokumentas / Mandatory, if exist at least one diagnostic/lab results docs -->
<!-- Nuoroda į paruoštus E027ats, E200ats, E014ats dokumentus / Reference to prepared E027ats, E200ats, E014ats document -->
<DocumentReference xmlns="http://hl7.org/fhir">
    
    <!-- 1..1 -->
    <!-- Dokumento antraštės identifikatorius Composition integraciniame taške / Doc identifier in Composition endpoint -->
    <masterIdentifier>
        <system value="Composition"/>
        <value value="12345679789"/>
    </masterIdentifier>
    
    <!-- 1..1 -->
    <!-- Patient of certificate -->
    <subject>
        <reference value="Patient/32856257683256/_history/2"/>
    </subject>
    
    <!-- 1..1 -->
    <!-- data-element-1840 -->
    <type>
        <coding>
            <!-- 1..1 -->
            <!-- Documento tipas / Document type by loinc. Document type it references to -->
            <!-- Classificator: doc-code -->
            <system value="http://loinc.org"/>
            <code value="11488-4"/>
        </coding>
        
        <!-- 1..1 -->
        <!-- Dokumento pavadinimas / Document title -->
        <text value="Atsakymas į siuntimą"/>
    </type>
    
    <!-- 1..1 -->
    <!-- Informacija apie dokumentą pateikusį specialistą / Info about specialist who has created this document -->
    <author>
        <reference value="Practitioner/12346/_history/1"/>
    </author>
    
    <!-- 1..1 -->
    <!-- Informacija apie dokumentą pateikusią SPĮ / Info about HIS who has created this document-->
    <custodian>
        <reference value="Organization/12346/_history/1"/>
    </custodian>
    
    <!-- 1..1 -->
    <!-- Document creation date -->
    <created value="2005-12-24T09:43:41+11:00"/>
    
    <!-- 1..1 -->
    <!-- When this reference was created. -->
    <indexed value="2005-12-24T09:43:41+11:00"/>
    
    <!-- 1..1 -->
    <!-- Nenaudojamas laukas, tačiau privalomas / Not used, but mandatory -->
    <!-- Nuorodos būsena / The status of this document reference -->
    <!-- Values: "current" (by default) -->
	<!-- Classificator: document-reference-status -->
    <status value="current"/>
    
    <!-- Medicininio dokumento būsena is kompozicijos būsenų + "signed" pasirašytam dokumentui / Medical document state derived from Composition states + "signed" status for signed docs -->
	<!-- Classificator: document-status -->
    <docStatus>
        <coding>
            <system value="http://esveikata.lt/classifiers/Document/Status"/>
            <code value="signed"/>
        </coding>
    </docStatus>
    
    <!-- 1..1 -->
    <!-- Dokumento mime tipas / The mime type of the source document. Currently unknown so text/xml by default -->
    <mimeType value="text/xml"/>
</DocumentReference>
