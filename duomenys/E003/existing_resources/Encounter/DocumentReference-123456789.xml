<?xml version="1.0" encoding="UTF-8"?>
<!-- 0..1 -->
<!-- Privaloma, jei draustumas kitoje šalyje / Mandaroty, if insurance in other country -->
<!-- Draudimo dokumentas / insurance document -->
<DocumentReference xmlns="http://hl7.org/fhir" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.w3.org/2005/Atom http://www.hl7.org/implement/standards/fhir/documentreference.xsd">
    
    <!-- Dokumento numeris (tuscias nes nenaudojamas) / Document number (empty as not  used) -->
    <masterIdentifier/>
    
    <!-- 1..1 -->
    <!-- Pacientas, kuriam priklauso draudimo dokumentas / The owner of insurance document -->
    <subject>
        <reference value="Patient/123456/_history/1"/>
    </subject>
    
    <!-- 1..1 -->
    <!-- Insurance Documment type it references to -->
	<!-- Classificator: insurance-doc-type -->
    <type>
        <coding>
            <system value="http://esveikata.lt/classifiers/InsuranceDocument/Type"/>
            <code value="1"/>
            <display value="Europos sveikatos draudimo kortelė"/>
        </coding>
    </type>
    
    <!-- 1..1 -->
    <!-- Dokumenta uzregistraves specialistas / Specialist registered insurance document -->
    <author>
        <reference value="Practitioner/12346/_history/1"/>
    </author>
    
    <!-- 1..1 -->
    <!-- When this reference was created. -->
    <indexed value="2005-12-24T09:43:41+11:00"/>
    
    <!-- 1..1 -->
    <!-- Nenaudojamas laukas, tačiau privalomas / Not used, but mandatory -->
    <!-- Nuorodos būsena / The status of this document reference -->
    <!-- Values: "current" (by default) -->
	<!-- Classificator: document-reference-status -->
    <status value="current"/>
    
    <!-- 1..1 -->
    <!-- The mime type of the source document. Currently unknown so text/plain by default -->
    <mimeType value="text/plain"/>
    
    <context>
        <period>
            <!-- 0..1 -->
            <!-- Draudimo dokumentas galioja iki / Insurance document expiration Date-->
            <end value="2004-12-23T08:01:00"/>
        </period>
    </context>
</DocumentReference>
