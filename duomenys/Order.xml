<Order xmlns="http://hl7.org/fhir" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.w3.org/2005/Atom http://www.hl7.org/implement/standards/fhir/order.xsd">
	
	<!-- 1..1 -->
	<!-- Skyrimo tipas / Order type -->
	<!-- Classificator:  -->
	<extension url="http://esveikata.lt/Profile/ltnhr-order#type">
		<valueCodeableConcept>
			<coding>
				<system value="http://esveikata.lt/classifiers/OrderType" />
				<!-- Values: "1" : Siuntimo konsultuoti, gydyti skyrimas | "2" : Laboratorinio tyrimo skyrimas | "3" : Patologijos tyrimo skyrimas | "4" : Skiepų skyrimas | "6" : Instrumentinio tyrimo skyrimas | "7" : Sistemoje neregistruotas siuntimas-->
				<code value="1" />
				<display value="Siuntimo konsultuoti, gydyti skyrimas" />
			</coding>
		</valueCodeableConcept>
	</extension>
	
	<!-- 0..1 -->
	<!-- Sveikatos priežiūros specialisto, sukūrusio siuntimą, profesinė kvalifikacija / Health Care Specialist‘s who made this order Qualification -->
	<extension url="http://esveikata.lt/Profile/ltnhr-order#sourceQualification">
		<valueCodeableConcept>
			<!-- 1..1 -->
			<!-- Specialisto prof. kvalifikacijos kodas / Target specialist prof qualification code -->
			<coding>
				<system value="http://esveikata.lt/classifiers/QualificationCode" />
				<code value="221227" />
				<display value="Gydytojas neurologas" />
			</coding>
		</valueCodeableConcept>
	</extension>
	
	<!-- 0..1 -->
	<!-- Sveikatos priežiūros specialisto, sukūrusio siuntimą, skyrius. Privalomas jeigu OrderType yra "7". / Department where specialists works. Mandatory, if OrderType value is "7". -->
	<extension url="http://esveikata.lt/Profile/ltnhr-order#sourceDepartment">
		<valueResource>
			<reference value="Organization/123546"/>
		</valueResource>
	</extension>
	
	<!-- 0..1 -->
	<!-- Specialisto, pas kurį siunčiama pacientas, prof. kvalifikacija arba skyriaus specializacija / Target specialist Qualification or target department Specialization -->
	<!-- data-element-325 -->
	<extension url="http://esveikata.lt/Profile/ltnhr-order#targetQualification">
		<valueCodeableConcept>
			
			<!-- 0..1 -->
			<!-- Specialisto prof. kvalifikacijos kodas / Target specialist prof qualification code -->
			<coding>
				<system value="http://esveikata.lt/classifiers/QualificationCode" />
				<code value="221227" />
				<display value="Gydytojas neurologas" />
			</coding>
			
			<!-- 0..1 -->
			<!-- Skyriaus specializacija (jeigu nenurodyta prof. kvalifikacija) / Department specialization (in case prof qualification is missing) -->
			<text value="Neurologijos skyrius"/>
		</valueCodeableConcept>
	</extension>
	
	<!-- 0..1 -->
	<!-- Nurodo, kad skyrimas yra ilgalaikis pagal nutylėjimą arba jeigu neturodyta skyrimas yra laikomas neilgalaikiu (false reikšmė) / Identifies long term orders (false by default or if not specified) -->
	<extension url="http://esveikata.lt/Profile/ltnhr-order#longTerm">
		<valueBoolean value = "false"/>
	</extension>
	
	<!-- 0..1 -->
	<!-- Patologijos tyrimų užsakymo preparato paėmimo būdas. / Specimen collection method for Pathology test order -->
	<extension url="http://esveikata.lt/Profile/ltnhr-order#specimenCollectionMethod">
		<valueCodeableConcept>
			<!-- Classificator: specimen-collection-method -->
			<coding>
				<system value="http://esveikata.lt/classifiers/Specimen/CollectionMethod"/>
				<!-- Values: "1" : Biopsija | "2" : Intraoperacinis tyrimas | "3" : Operacinė medžiaga | "4" : Autopsija -->
				<code value="2"/>
				<display value="Intraoperacinis tyrimas"/>
			</coding>
		</valueCodeableConcept>
	</extension>
	
	<!-- 0..1 -->
	<!-- Tiriamos medžiagos tipas ir aprašymas. Ėminio patikslinimas įvedamas į text lauką. -->
	<extension url="http://esveikata.lt/Profile/ltnhr-order#specimenType">
		<valueCodeableConcept>
			<coding>
				<!-- 1..1 -->
				<!-- Tiriamos medžiagos tipas / Kind of material that forms the specimen -->
				<!-- Classificator: D.4.47. Ėminių klasifikatorius -->
				<system value="http://esveikata.lt/classifiers/Specimen/Type"/>
				<code value="123456"/>
				<display value="Tiriamos medžiagos tipas"/>
			</coding>
			
			<!-- 0..1 -->
			<!-- Privalomas, jei „Ėminys“ nurodyta „Kita“ / Mandatory, if spacemen value is "Other" -->
			<!-- Ėminio patikslinimas  / Specimen description -->
			<text value="Left hand blood"/>
		</valueCodeableConcept>
	</extension>
	
	<!-- 0..1 -->
	<!-- Pageidaujamo tyrimo ar tyrimo skyrimo kodas (coding laukas). -->
	<!-- Classificator: diagnostic-type -->
	<extension url="http://esveikata.lt/Profile/ltnhr-order#diagnosticItemCode">
		<valueCodeableConcept>
			<coding>
				<system value="http://esveikata.lt/classifiers/DiagnosticOrder/ItemCode"/>
				<code value="123456"/>
				<!-- 0..1 -->
				<!-- Tyrimo pavadinimas / Test name  -->
				<display value="Laboratorinio tyrimo kažkoks tyrimas"/>
			</coding>
		</valueCodeableConcept>
	</extension>
	
	<!-- 0..1 -->
	<!-- Sistemoje neregistruotame siuntime nurodyta diagnozė. Privalomas jeigu OrderType yra "7". Mandatory, if OrderType value is "7". -->
	<!-- Classificator: tlk-10-am -->
	<extension url="http://esveikata.lt/Profile/ltnhr-order#paperOrderCondition">
		<valueCodeableConcept>
			<coding>
				<system value="http://esveikata.lt/classifiers/TLK-10-AM"/>
				<code value="1101"/>
				<!-- Diagnozės pavadinimas -->
				<display value="APENDICITAS"/>
			</coding>
		</valueCodeableConcept>
	</extension>
	
	<!-- 1..1 -->
	<!-- Siuntimo data / Order issue date -->
	<!-- data-element-1910 -->
	<date value="2014-02-21T09:03:04+11:00"/>
	
	<!-- 1..1 -->
	<!-- Nuoroda į paciento, kuriam sukurtas siuntimas, resursą / Patient who has been ordered -->
	<!-- data-element-1 -->
	<subject>
		<reference value="Patient/123456789/_history/1"/>
	</subject>
	
	<!-- 1..1 -->
	<!-- Nuoroda į sveikatos priežiūros specialisto, kuris sukūrė siuntimą, resursą / Health care specialist who made this order -->
	<!-- data-element-20 -->
	<source>
		<reference value="Practitioner/987654321/_history/1"/>
	</source>
	
	<!-- 0..1 -->
	<!-- Nurodo SPĮ arba specialistą, kuriam yra priskirtą užduotis. / Practitioner or Organization who has been assigned to this task Order -->
	<target>
		<reference value="Practitioner/123546/_history/1"/>
	</target>
	
	<!-- 0..1 -->
	<!-- Siuntimo diagnozių sąrašas / List of Referral Diagnosis -->
	<!-- data-element-330 -->
	<reasonResource>
		<reference value="List/123456/_history/1"/>
	</reasonResource>
	
	<!-- 1..1 -->
	<!-- Atvykimo duomenys, kurių pagrindu buvo sukurtas šis skyrimo resursas / Encounter where this Order was created at -->
	<detail>
		<reference value="Encounter/12345689/_history/1"/>
	</detail>
	
	<!-- 0..1 -->
	<!-- Detalūs paskyrimo duomenys / Detailed info about what is ordered and its status -->
	<detail>
		<reference value="DiagnosticOrder/12345689/_history/1"/>
	</detail>
</Order> 